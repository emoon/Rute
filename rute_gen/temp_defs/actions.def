///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// Actions

struct Action {
	is_enabled() -> bool,
	set_text(String text),
	text() -> String,
	[manual] set_shortcut(Keys key),
	[manual] set_shortcut_mod(Keys key, MetaKeys modifier),
	[callback] triggered(),
	[manual] set_int_data(i32 data),
	[manual] get_int_data() -> i32,
}

