struct ListWidget : Widget {
    add_item(label: String),
    clear(),
    /// Returns a list of all selected items in the list widget.
    selected_items() -> [*ListWidgetItem],
    //[event] item_pressed(item: &ListWidgetItemType),
    [org_name(addItem)]
    add_widget_item(item: *ListWidgetItemType),

    ///
    /// Returns the drop actions supported by this view.
    ///
    /// **See also:** [`ListWidget::drop_actions()`]
    [event] supported_drop_actions() -> Rute::DropActions,
}

