
struct PlatformWindow {
    [replace] initialize(),
    window() -> Window?,
    parent() -> PlatformWindow?,
    screen() -> PlatformScreen?,
    [replace] format() -> SurfaceFormat,
    [replace] frame_margins() -> Margins,
    [replace] safe_area_margins() -> Margins,
    [replace] set_visible(visible: bool),
    [replace] set_window_flags(flags: Rute::WindowFlags),
    [replace] set_window_state(state: Rute::WindowStates),
    [replace] win_id() -> WId,
    [replace] set_parent(window: *PlatformWindowType),
    [replace] set_window_title(title: String),
    [replace] set_window_file_path(title: String),
    [replace] set_window_icon(icon: &IconType),
    [replace] raise(),
    [replace] lower(),
    [replace] is_exposed() -> bool,
    [replace] is_active() -> bool,
    [replace] is_ancestor_of(child: *PlatformWindowType) -> bool,
    [replace] is_embedded() -> bool,
    [replace] is_foreign_window() -> bool,
    [replace] map_to_global(pos: &PointType) -> Point,
    [replace] map_from_global(pos: &PointType) -> Point,
    [replace] propagate_size_hints(),
    [replace] set_opacity(level: f32),
    [replace] set_mask(region: &RegionType),
    [replace] request_activate_window(),
    [replace] handle_content_orientation_change(orientation: Rute::ScreenOrientation),
    [replace] device_pixel_ratio() -> f32,
    [replace] set_keyboard_grab_enabled(grab: bool) -> bool,
    [replace] set_mouse_grab_enabled(grab: bool) -> bool,
    [replace] set_window_modified(modified: bool) -> bool,
    [replace] window_event(event: *EventType),
    [replace] start_system_resize(pos: &PointType, corner: Rute::Corner) -> bool,
    [replace] start_system_move(pos: &PointType) -> bool,
    [replace] set_alert_state(enabled: bool),
    [replace] is_alert_state() -> bool,
    [replace] invalidate_surface(),
    [replace] request_update(),
    window_minimum_size() -> Size,
    window_maximum_size() -> Size,
    window_base_size() -> Size,
    window_size_increment() -> Size,
    [static] format_window_title(title: String, separator: String) -> String,
}

