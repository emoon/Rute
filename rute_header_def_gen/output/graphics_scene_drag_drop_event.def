struct GraphicsSceneDragDropEvent : GraphicsSceneEvent {
    pos() -> PointFType,
    set_pos(pos: &PointFType),
    scene_pos() -> PointFType,
    set_scene_pos(pos: &PointFType),
    screen_pos() -> PointType,
