struct GraphicsSceneDragDropEvent : GraphicsSceneEvent {
    pos() -> PointFType,
    set_pos(&PointFType pos),
    scene_pos() -> PointFType,
    set_scene_pos(&PointFType pos),
    screen_pos() -> PointType,
    set_screen_pos(&PointType pos),
    buttons() -> Rute::MouseButtons,
    set_buttons(Rute::MouseButtons buttons),
    modifiers() -> Rute::KeyboardModifiers,
    set_modifiers(Rute::KeyboardModifiers modifiers),
    possible_actions() -> Rute::DropActions,
    set_possible_actions(Rute::DropActions actions),
    proposed_action() -> Rute::DropAction,
    set_proposed_action(Rute::DropAction action),
    accept_proposed_action(),
    drop_action() -> Rute::DropAction,
    set_drop_action(Rute::DropAction action),
    source() -> &WidgetType,
    set_source(&WidgetType source),
    mime_data() -> &MimeDataType,
    set_mime_data(&MimeDataType data),
}

