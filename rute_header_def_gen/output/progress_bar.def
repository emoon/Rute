struct ProgressBar : Widget {
    minimum() -> i32,
    maximum() -> i32,
    value() -> i32,
    text() -> String,
    set_text_visible(bool visible),
    is_text_visible() -> bool,
    alignment() -> Rute::Alignment,
    set_alignment(Rute::Alignment alignment),
    size_hint() -> SizeType,
    minimum_size_hint() -> SizeType,
    orientation() -> Rute::Orientation,
    set_inverted_appearance(bool invert),
    inverted_appearance() -> bool,
    set_text_direction(ProgressBar::Direction text_direction),
    text_direction() -> ProgressBar::Direction,
    set_format(String format),
    reset_format(),
    format() -> String,
    reset(),
    set_range(i32 minimum, i32 maximum),
    set_minimum(i32 minimum),
    set_maximum(i32 maximum),
    set_value(i32 value),
    set_orientation(Rute::Orientation arg0),
    value_changed(i32 value),
    event(&EventType e) -> bool,
    paint_event(&PaintEventType arg0),
    init_style_option(&StyleOptionProgressBarType option),
}

