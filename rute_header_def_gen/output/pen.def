
struct Pen {
    swap(&PenType other),
    style() -> Rute::PenStyle,
    set_style(Rute::PenStyle arg0),
    dash_pattern() -> Vector<qreal>Type,
    set_dash_pattern(&Vector<qreal>Type pattern),
    dash_offset() -> f32,
    set_dash_offset(f32 doffset),
    miter_limit() -> f32,
    set_miter_limit(f32 limit),
    width_f() -> f32,
    set_width_f(f32 width),
    width() -> i32,
    set_width(i32 width),
    color() -> ColorType,
    set_color(&ColorType color),
    brush() -> BrushType,
    set_brush(&BrushType brush),
    is_solid() -> bool,
    cap_style() -> Rute::PenCapStyle,
    set_cap_style(Rute::PenCapStyle pcs),
    join_style() -> Rute::PenJoinStyle,
    set_join_style(Rute::PenJoinStyle pcs),
    is_cosmetic() -> bool,
    set_cosmetic(bool cosmetic),
    is_detached() -> bool,
}

