struct GroupBox : Widget {
    title() -> String,
    set_title(title: String),
    alignment() -> Rute::Alignment,
    set_alignment(alignment: i32),
    minimum_size_hint() -> SizeType,
    is_flat() -> bool,
    set_flat(flat: bool),
    is_checkable() -> bool,
    set_checkable(checkable: bool),
    is_checked() -> bool,
    set_checked(checked: bool),
    [callback] clicked(checked: bool),
    [callback] toggled(arg0: bool),
    event(event: &EventType) -> bool,
    child_event(event: &ChildEventType),
    resize_event(event: &ResizeEventType),
    paint_event(event: &PaintEventType),
    focus_in_event(event: &FocusEventType),
    change_event(event: &EventType),
    mouse_press_event(event: &MouseEventType),
    mouse_move_event(event: &MouseEventType),
    mouse_release_event(event: &MouseEventType),
    init_style_option(option: &StyleOptionGroupBoxType),
}

