struct GroupBox : Widget {
    title() -> String,
    set_title(title: String),
    alignment() -> Rute::Alignment,
    set_alignment(alignment: i32),
    [override] minimum_size_hint() -> Size,
    is_flat() -> bool,
    set_flat(flat: bool),
    is_checkable() -> bool,
    set_checkable(checkable: bool),
    is_checked() -> bool,
    set_checked(checked: bool),
    [event] clicked(checked: bool),
    [event] toggled(arg0: bool),
    [override] event(event: &EventType) -> bool,
    [override] child_event(event: &ChildEventType),
    [override] resize_event(event: &ResizeEventType),
    [override] paint_event(event: &PaintEventType),
    [override] focus_in_event(event: &FocusEventType),
    [override] change_event(event: &EventType),
    [override] mouse_press_event(event: &MouseEventType),
    [override] mouse_move_event(event: &MouseEventType),
    [override] mouse_release_event(event: &MouseEventType),
    init_style_option(option: &StyleOptionGroupBoxType),
}

