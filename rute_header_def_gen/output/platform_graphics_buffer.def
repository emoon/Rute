[org_name(QPlatformGraphicsBuffer)]
enum AccessType {
    None,
    SWReadAccess,
    SWWriteAccess,
    TextureAccess,
    HWCompositor,
}

[org_name(QPlatformGraphicsBuffer)]
enum Origin {
    OriginBottomLeft,
    OriginTopLeft,
}

struct PlatformGraphicsBuffer : Object {
    is_locked() -> AccessTypes,
    lock(access: AccessTypes, rect: &RectType) -> bool,
    unlock(),
    [event] bind_to_texture(rect: &RectType) -> bool,
    [event] data() -> uchar?,
    [event] data() -> uchar?,
    [event] bytes_per_line() -> i32,
    byte_count() -> i32,
    [event] origin() -> PlatformGraphicsBuffer::Origin,
    size() -> Size,
    format() -> PixelFormat,
    [signal] unlocked(previous_access_types: AccessTypes),
    [event] do_lock(access: AccessTypes, rect: &RectType) -> bool,
    [event] do_unlock(),
}

