    horizontal_spacing() -> f32,
    vertical_spacing() -> f32,
    remove_at(index: i32),
    count() -> i32,
    item_at(index: i32) -> &GraphicsLayoutItemType,
    invalidate(),
    size_hint(which: Rute::SizeHint, constraint: &SizeFType) -> SizeFType,
}

