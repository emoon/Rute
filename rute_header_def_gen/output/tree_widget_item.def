
struct TreeWidgetItem {
    clone() -> &TreeWidgetItemType,
    set_selected(bool select),
    is_selected() -> bool,
    set_hidden(bool hide),
    is_hidden() -> bool,
    set_expanded(bool expand),
    is_expanded() -> bool,
    set_first_column_spanned(bool span),
    is_first_column_spanned() -> bool,
    set_disabled(bool disabled),
    is_disabled() -> bool,
    set_child_indicator_policy(TreeWidgetItem::ChildIndicatorPolicy policy),
    child_indicator_policy() -> TreeWidgetItem::ChildIndicatorPolicy,
    flags() -> Rute::ItemFlags,
    set_flags(Rute::ItemFlags flags),
    text(i32 column) -> String,
    set_text(i32 column, String text),
    icon(i32 column) -> IconType,
    set_icon(i32 column, &IconType icon),
    status_tip(i32 column) -> String,
    set_status_tip(i32 column, String status_tip),
    tool_tip(i32 column) -> String,
    set_tool_tip(i32 column, String tool_tip),
    whats_this(i32 column) -> String,
    set_whats_this(i32 column, String whats_this),
    font(i32 column) -> FontType,
    set_font(i32 column, &FontType font),
    text_alignment(i32 column) -> i32,
    set_text_alignment(i32 column, i32 alignment),
    background_color(i32 column) -> ColorType,
    set_background_color(i32 column, &ColorType color),
    background(i32 column) -> BrushType,
    set_background(i32 column, &BrushType brush),
    text_color(i32 column) -> ColorType,
    set_text_color(i32 column, &ColorType color),
    foreground(i32 column) -> BrushType,
    set_foreground(i32 column, &BrushType brush),
    check_state(i32 column) -> Rute::CheckState,
    set_check_state(i32 column, Rute::CheckState state),
    size_hint(i32 column) -> SizeType,
    set_size_hint(i32 column, &SizeType size),
    data(i32 column, i32 role) -> VariantType,
    set_data(i32 column, i32 role, &VariantType value),
    read(&DataStreamType in),
    write(&DataStreamType out),
    parent() -> &TreeWidgetItemType,
    child(i32 index) -> &TreeWidgetItemType,
    child_count() -> i32,
    column_count() -> i32,
    index_of_child(&TreeWidgetItemType child) -> i32,
    add_child(&TreeWidgetItemType child),
    insert_child(i32 index, &TreeWidgetItemType child),
    remove_child(&TreeWidgetItemType child),
    take_child(i32 index) -> &TreeWidgetItemType,
    add_children(&List<QTreeWidgetItemType children),
    insert_children(i32 index, &List<QTreeWidgetItemType children),
    take_children() -> <&TreeWidgetItemType>,
    type() -> i32,
    sort_children(i32 column, Rute::SortOrder order),
    emit_data_changed(),
    sort_children(i32 column, Rute::SortOrder order, bool climb),
    children_check_state(i32 column) -> VariantType,
    item_changed(),
    execute_pending_sort(),
}

