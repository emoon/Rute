struct BasicKeyEventTransition : AbstractTransition {
    event_type() -> Event::Type,
    set_event_type(Event::Type type),
    key() -> i32,
    set_key(i32 key),
    modifier_mask() -> Rute::KeyboardModifiers,
    set_modifier_mask(Rute::KeyboardModifiers modifiers),
    event_test(&EventType event) -> bool,
    on_transition(&EventType arg0),
