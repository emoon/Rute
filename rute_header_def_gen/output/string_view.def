
struct StringView {
    size() -> i32,
    data() -> const_pointer,
    utf16() -> &storage_type,
    to_latin1() -> ByteArrayType,
    to_utf8() -> ByteArrayType,
    to_local8_bit() -> ByteArrayType,
    to_ucs4() -> [uint],
    at(n: i32) -> CharType,
    mid(pos: i32) -> StringViewType,
    left(n: i32) -> StringViewType,
    right(n: i32) -> StringViewType,
    chopped(n: i32) -> StringViewType,
    chop(n: i32),
    starts_with(s: StringViewType, cs: Rute::CaseSensitivity) -> bool,
    starts_with(s: Latin1StringType, cs: Rute::CaseSensitivity) -> bool,
    starts_with(c: CharType) -> bool,
    starts_with(c: CharType, cs: Rute::CaseSensitivity) -> bool,
    ends_with(s: StringViewType, cs: Rute::CaseSensitivity) -> bool,
    ends_with(s: Latin1StringType, cs: Rute::CaseSensitivity) -> bool,
    ends_with(c: CharType) -> bool,
    ends_with(c: CharType, cs: Rute::CaseSensitivity) -> bool,
    begin() -> const_iterator,
    end() -> const_iterator,
    cbegin() -> const_iterator,
    cend() -> const_iterator,
    rbegin() -> const_reverse_iterator,
    rend() -> const_reverse_iterator,
    crbegin() -> const_reverse_iterator,
    crend() -> const_reverse_iterator,
    empty() -> bool,
    front() -> CharType,
    back() -> CharType,
    is_null() -> bool,
    is_empty() -> bool,
    length() -> i32,
    first() -> CharType,
    last() -> CharType,
}

