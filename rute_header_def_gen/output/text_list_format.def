struct TextListFormat : TextFormat {
    is_valid() -> bool,
    set_style(TextListFormat::Style style),
    style() -> TextListFormat::Style,
    set_indent(i32 indent),
    indent() -> i32,
    set_number_prefix(String number_prefix),
    number_prefix() -> String,
    set_number_suffix(String number_suffix),
    number_suffix() -> String,
}

