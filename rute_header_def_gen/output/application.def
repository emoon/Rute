struct Application : GuiApplication {
    style() -> &StyleType,
    set_style(&StyleType arg0),
    set_style(String arg0) -> &StyleType,
    color_spec() -> i32,
    set_color_spec(i32 arg0),
    palette(&WidgetType arg0) -> PaletteType,
    palette(&char class_name) -> PaletteType,
    set_palette(&PaletteType arg0, &char class_name),
    font() -> FontType,
    font(&WidgetType arg0) -> FontType,
    font(&char class_name) -> FontType,
    set_font(&FontType arg0, &char class_name),
    set_window_icon(&IconType icon),
    window_icon() -> IconType,
    all_widgets() -> WidgetListType,
    top_level_widgets() -> WidgetListType,
    desktop() -> &DesktopWidgetType,
    active_popup_widget() -> &WidgetType,
    active_modal_widget() -> &WidgetType,
    focus_widget() -> &WidgetType,
    active_window() -> &WidgetType,
    set_active_window(&WidgetType act),
    widget_at(&PointType p) -> &WidgetType,
    widget_at(i32 x, i32 y) -> &WidgetType,
    top_level_at(&PointType p) -> &WidgetType,
    top_level_at(i32 x, i32 y) -> &WidgetType,
    beep(),
    alert(&WidgetType widget, i32 duration),
    set_cursor_flash_time(i32 arg0),
    cursor_flash_time() -> i32,
    set_double_click_interval(i32 arg0),
    double_click_interval() -> i32,
    set_keyboard_input_interval(i32 arg0),
    keyboard_input_interval() -> i32,
    set_wheel_scroll_lines(i32 arg0),
    wheel_scroll_lines() -> i32,
    set_start_drag_time(i32 ms),
    start_drag_time() -> i32,
    set_start_drag_distance(i32 l),
    start_drag_distance() -> i32,
    is_effect_enabled(Rute::UIEffect arg0) -> bool,
    set_effect_enabled(Rute::UIEffect arg0, bool enable),
    exec() -> i32,
    notify(&ObjectType arg0, &EventType arg1) -> bool,
    focus_changed(&WidgetType old, &WidgetType now),
    style_sheet() -> String,
    [callback] set_style_sheet(String sheet),
    [callback] set_auto_sip_enabled(bool enabled),
    [callback] auto_sip_enabled() -> bool,
    [callback] close_all_windows(),
    [callback] about_qt(),
    event(&EventType arg0) -> bool,
    compress_event(&EventType arg0, &ObjectType receiver, &PostEventListType arg1) -> bool,
}

