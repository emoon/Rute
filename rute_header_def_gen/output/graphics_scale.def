struct GraphicsScale : GraphicsTransform {
    origin() -> Vector3DType,
    set_origin(point: &Vector3DType),
    x_scale() -> f32,
    set_x_scale(arg0: f32),
    y_scale() -> f32,
    set_y_scale(arg0: f32),
    z_scale() -> f32,
    set_z_scale(arg0: f32),
    apply_to(matrix: &Matrix4x4Type),
    [callback] origin_changed(),
    [callback] x_scale_changed(),
    [callback] y_scale_changed(),
    [callback] z_scale_changed(),
    [callback] scale_changed(),
}

