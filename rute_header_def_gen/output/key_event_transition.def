struct KeyEventTransition : EventTransition {
    key() -> i32,
    set_key(key: i32),
    modifier_mask() -> Rute::KeyboardModifiers,
    set_modifier_mask(modifiers: Rute::KeyboardModifiers),
    [event] on_transition(event: *EventType),
    [event] event_test(event: *EventType) -> bool,
}

