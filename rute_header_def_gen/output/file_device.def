struct FileDevice : IODevice {
    error() -> FileDevice::FileError,
    unset_error(),
    close(),
    is_sequential() -> bool,
    handle() -> i32,
    file_name() -> String,
    pos() -> i64,
    seek(i64 offset) -> bool,
    at_end() -> bool,
    flush() -> bool,
    size() -> i64,
    resize(i64 sz) -> bool,
    permissions() -> Permissions,
    set_permissions(Permissions permission_spec) -> bool,
    map(i64 offset, i64 size, FileDevice::MemoryMapFlags flags) -> &uchar,
    unmap(&uchar address) -> bool,
    file_time(FileDevice::FileTime time) -> DateTimeType,
    set_file_time(&DateTimeType new_date, FileDevice::FileTime file_time) -> bool,
    read_data(&char data, i64 maxlen) -> i64,
    write_data(&char data, i64 len) -> i64,
    read_line_data(&char data, i64 maxlen) -> i64,
}

