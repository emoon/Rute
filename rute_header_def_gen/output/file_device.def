struct FileDevice : IODevice {
    error() -> FileDevice::FileError,
    unset_error(),
    close(),
    is_sequential() -> bool,
    handle() -> i32,
    file_name() -> String,
    pos() -> i64,
    seek(offset: i64) -> bool,
    at_end() -> bool,
    flush() -> bool,
    size() -> i64,
    resize(sz: i64) -> bool,
    permissions() -> Permissions,
    set_permissions(permission_spec: Permissions) -> bool,
    map(offset: i64, size: i64, flags: FileDevice::MemoryMapFlags) -> &uchar,
    unmap(address: &uchar) -> bool,
    file_time(time: FileDevice::FileTime) -> DateTimeType,
    set_file_time(new_date: &DateTimeType, file_time: FileDevice::FileTime) -> bool,
    read_data(data: &char, maxlen: i64) -> i64,
    write_data(data: &char, len: i64) -> i64,
    read_line_data(data: &char, maxlen: i64) -> i64,
}

