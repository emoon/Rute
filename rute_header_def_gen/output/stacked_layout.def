struct StackedLayout : Layout {
    add_widget(w: &WidgetType) -> i32,
    insert_widget(index: i32, w: &WidgetType) -> i32,
    current_widget() -> &WidgetType,
    current_index() -> i32,
    widget(arg0: i32) -> &WidgetType,
    count() -> i32,
    stacking_mode() -> StackedLayout::StackingMode,
    set_stacking_mode(stacking_mode: StackedLayout::StackingMode),
    add_item(item: &LayoutItemType),
    size_hint() -> SizeType,
    minimum_size() -> SizeType,
    item_at(arg0: i32) -> &LayoutItemType,
    take_at(arg0: i32) -> &LayoutItemType,
    has_height_for_width() -> bool,
    height_for_width(width: i32) -> i32,
    [callback] widget_removed(index: i32),
    [callback] current_changed(index: i32),
    set_current_index(index: i32),
    set_current_widget(w: &WidgetType),
}

