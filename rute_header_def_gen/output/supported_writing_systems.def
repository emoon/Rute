struct SupportedWritingSystems {
    /// 
    /// Sets or clears support for the specified *writingSystem* based on the
    /// value given by *support.*
    set_supported(arg0: FontDatabase::WritingSystem, supported: bool),
    /// 
    /// Returns `true` if the writing system specified by *writingSystem* is
    /// supported; otherwise returns `false.`
    supported(arg0: FontDatabase::WritingSystem) -> bool,
}

