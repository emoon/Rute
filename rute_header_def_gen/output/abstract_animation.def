struct AbstractAnimation : Object {
    state() -> AbstractAnimation::State,
    group() -> &AnimationGroupType,
    direction() -> AbstractAnimation::Direction,
    set_direction(AbstractAnimation::Direction direction),
    current_time() -> i32,
    current_loop_time() -> i32,
    loop_count() -> i32,
    set_loop_count(i32 loop_count),
    current_loop() -> i32,
    duration() -> i32,
    total_duration() -> i32,
    finished(),
    state_changed(AbstractAnimation::State new_state, AbstractAnimation::State old_state),
    current_loop_changed(i32 current_loop),
    direction_changed(AbstractAnimation::Direction arg0),
    start(AbstractAnimation::DeletionPolicy policy),
    pause(),
    resume(),
    set_paused(bool arg0),
    stop(),
    set_current_time(i32 msecs),
    event(&EventType event) -> bool,
    update_current_time(i32 current_time),
    update_state(AbstractAnimation::State new_state, AbstractAnimation::State old_state),
    update_direction(AbstractAnimation::Direction direction),
}

