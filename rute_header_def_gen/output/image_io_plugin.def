#[org_name(QImageIOPlugin)]
enum Capability {
    CanRead,
    CanWrite,
    CanReadIncremental,
}

struct ImageIOPlugin : Object {
    [replace] capabilities(device: &IODeviceType, format: &ByteArrayType) -> Capabilities,
    [replace] create(device: &IODeviceType, format: &ByteArrayType) -> ImageIOHandler?,
}

