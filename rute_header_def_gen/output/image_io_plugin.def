[org_name(QImageIOPlugin)]
enum Capability {
    CanRead,
    CanWrite,
    CanReadIncremental,
}

struct ImageIOPlugin : Object {
    [event] capabilities(device: *IODeviceType, format: &ByteArrayType) -> Capabilities,
    [event] create(device: *IODeviceType, format: &ByteArrayType) -> ImageIOHandler?,
}

