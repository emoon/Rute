struct GraphicsBlurEffect : GraphicsEffect {
    bounding_rect_for(rect: &RectFType) -> RectFType,
    blur_radius() -> f32,
    blur_hints() -> BlurHints,
    set_blur_radius(blur_radius: f32),
    set_blur_hints(hints: BlurHints),
    [callback] blur_radius_changed(blur_radius: f32),
    [callback] blur_hints_changed(hints: BlurHints),
    draw(painter: &PainterType),
}

