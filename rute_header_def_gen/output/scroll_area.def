struct ScrollArea : AbstractScrollArea {
    widget() -> Widget?,
    set_widget(widget: &WidgetType),
    take_widget() -> Widget?,
    widget_resizable() -> bool,
    set_widget_resizable(resizable: bool),
    [override] size_hint() -> Size,
    [override] focus_next_prev_child(next: bool) -> bool,
    alignment() -> Rute::Alignment,
    set_alignment(arg0: Rute::Alignment),
    ensure_visible(x: i32, y: i32, xmargin: i32, ymargin: i32),
    ensure_widget_visible(child_widget: &WidgetType, xmargin: i32, ymargin: i32),
    [override] event(arg0: &EventType) -> bool,
    [override] event_filter(arg0: &ObjectType, arg1: &EventType) -> bool,
    [override] resize_event(arg0: &ResizeEventType),
    [override] scroll_contents_by(dx: i32, dy: i32),
    [override] viewport_size_hint() -> Size,
}

