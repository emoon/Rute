struct SplitterHandle : Widget {
    set_orientation(o: Rute::Orientation),
    orientation() -> Rute::Orientation,
    opaque_resize() -> bool,
    splitter() -> &SplitterType,
    size_hint() -> SizeType,
    paint_event(arg0: &PaintEventType),
    mouse_move_event(arg0: &MouseEventType),
    mouse_press_event(arg0: &MouseEventType),
    mouse_release_event(arg0: &MouseEventType),
    resize_event(arg0: &ResizeEventType),
    event(arg0: &EventType) -> bool,
    move_splitter(p: i32),
    closest_legal_position(p: i32) -> i32,
}

