struct Clipboard : Object {
    clear(Clipboard::Mode mode),
    supports_selection() -> bool,
    supports_find_buffer() -> bool,
    owns_selection() -> bool,
    owns_clipboard() -> bool,
    owns_find_buffer() -> bool,
    text(Clipboard::Mode mode) -> String,
    text(String subtype, Clipboard::Mode mode) -> String,
    set_text(String arg0, Clipboard::Mode mode),
    mime_data(Clipboard::Mode mode) -> &MimeDataType,
    set_mime_data(&MimeDataType data, Clipboard::Mode mode),
    image(Clipboard::Mode mode) -> ImageType,
    pixmap(Clipboard::Mode mode) -> PixmapType,
    set_image(&ImageType arg0, Clipboard::Mode mode),
    set_pixmap(&PixmapType arg0, Clipboard::Mode mode),
    changed(Clipboard::Mode mode),
    selection_changed(),
    find_buffer_changed(),
    data_changed(),
    supports_mode(Clipboard::Mode mode) -> bool,
    owns_mode(Clipboard::Mode mode) -> bool,
    emit_changed(Clipboard::Mode mode),
}

