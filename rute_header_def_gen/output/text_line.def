
struct TextLine {
    is_valid() -> bool,
    rect() -> RectFType,
    x() -> f32,
    y() -> f32,
    width() -> f32,
    ascent() -> f32,
    descent() -> f32,
    height() -> f32,
    leading() -> f32,
    set_leading_included(bool included),
    leading_included() -> bool,
    natural_text_width() -> f32,
    horizontal_advance() -> f32,
    natural_text_rect() -> RectFType,
    cursor_to_x(&i32 cursor_pos, TextLine::Edge edge) -> f32,
    cursor_to_x(i32 cursor_pos, TextLine::Edge edge) -> f32,
    x_to_cursor(f32 x, TextLine::CursorPosition arg0) -> i32,
    set_line_width(f32 width),
    set_num_columns(i32 columns),
    set_num_columns(i32 columns, f32 alignment_width),
    set_position(&PointFType pos),
    position() -> PointFType,
    text_start() -> i32,
    text_length() -> i32,
    line_number() -> i32,
    draw(&PainterType p, &PointFType point, onst QTextLayout::FormatRange * selection),
    glyph_runs(i32 from, i32 length) -> <GlyphRunType>,
    layout_helper(i32 num_glyphs),
}

