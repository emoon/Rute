
struct FontInfo {
    swap(&FontInfoType other),
    family() -> String,
    style_name() -> String,
    pixel_size() -> i32,
    point_size() -> i32,
    point_size_f() -> f32,
    italic() -> bool,
    style() -> Font::Style,
    weight() -> i32,
    bold() -> bool,
    underline() -> bool,
    overline() -> bool,
    fixed_pitch() -> bool,
    style_hint() -> Font::StyleHint,
    raw_mode() -> bool,
    exact_match() -> bool,
}

