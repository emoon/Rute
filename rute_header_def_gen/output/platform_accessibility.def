struct PlatformAccessibility {
    [event] notify_accessibility_update(event: *AccessibleEventType),
    [event] set_root_object(o: *ObjectType),
    [event] initialize(),
    [event] cleanup(),
    is_active() -> bool,
    set_active(active: bool),
}

