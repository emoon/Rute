struct DockWidgetLayout : Layout {
    add_item(&LayoutItemType item),
    item_at(i32 index) -> &LayoutItemType,
    take_at(i32 index) -> &LayoutItemType,
    count() -> i32,
    maximum_size() -> SizeType,
    minimum_size() -> SizeType,
    size_hint() -> SizeType,
    size_from_content(&SizeType content, bool floating) -> SizeType,
    widget_for_role(DockWidgetLayout::Role r) -> &WidgetType,
    set_widget_for_role(DockWidgetLayout::Role r, &WidgetType w),
    item_for_role(DockWidgetLayout::Role r) -> &LayoutItemType,
    title_area() -> RectType,
    minimum_title_width() -> i32,
    title_height() -> i32,
    update_max_size(),
    wm_supports_native_window_deco() -> bool,
    native_window_deco() -> bool,
    native_window_deco(bool floating) -> bool,
    set_vertical_title_bar(bool b),
}

