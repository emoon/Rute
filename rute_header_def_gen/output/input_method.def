struct InputMethod : Object {
    input_item_transform() -> TransformType,
    set_input_item_transform(&TransformType transform),
    input_item_rectangle() -> RectFType,
    set_input_item_rectangle(&RectFType rect),
    cursor_rectangle() -> RectFType,
    anchor_rectangle() -> RectFType,
    keyboard_rectangle() -> RectFType,
    input_item_clip_rectangle() -> RectFType,
    is_visible() -> bool,
    set_visible(bool visible),
    is_animating() -> bool,
    locale() -> LocaleType,
    input_direction() -> Rute::LayoutDirection,
    query_focus_object(Rute::InputMethodQuery query, VariantType argument) -> VariantType,
    show(),
    hide(),
    update(Rute::InputMethodQueries queries),
    reset(),
    commit(),
    invoke_action(InputMethod::Action a, i32 cursor_position),
    [callback] cursor_rectangle_changed(),
    [callback] anchor_rectangle_changed(),
    [callback] keyboard_rectangle_changed(),
    [callback] input_item_clip_rectangle_changed(),
    [callback] visible_changed(),
    [callback] animating_changed(),
    [callback] locale_changed(),
    [callback] input_direction_changed(Rute::LayoutDirection new_direction),
}

