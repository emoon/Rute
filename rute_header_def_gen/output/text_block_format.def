struct TextBlockFormat : TextFormat {
    is_valid() -> bool,
    set_alignment(Rute::Alignment alignment),
    alignment() -> Rute::Alignment,
    set_top_margin(f32 margin),
    top_margin() -> f32,
    set_bottom_margin(f32 margin),
    bottom_margin() -> f32,
    set_left_margin(f32 margin),
    left_margin() -> f32,
    set_right_margin(f32 margin),
    right_margin() -> f32,
    set_text_indent(f32 aindent),
    text_indent() -> f32,
    set_indent(i32 indent),
    indent() -> i32,
    set_line_height(f32 height, i32 height_type),
    line_height(f32 script_line_height, f32 scaling) -> f32,
    line_height() -> f32,
    line_height_type() -> i32,
    set_non_breakable_lines(bool b),
    non_breakable_lines() -> bool,
    set_page_break_policy(PageBreakFlags flags),
    page_break_policy() -> PageBreakFlags,
    set_tab_positions(onst QList<QTextOption::Tab> & tabs),
    tab_positions() -> List<QTextOption::Tab>,
}

