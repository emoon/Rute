struct ProxyStyle : CommonStyle {
    base_style() -> Style?,
    set_base_style(style: &StyleType),
    [override] draw_primitive(element: Style::PrimitiveElement, option: &StyleOptionType, painter: &PainterType, widget: &WidgetType),
    [override] draw_item_text(painter: &PainterType, rect: &RectType, flags: i32, pal: &PaletteType, enabled: bool, text: String, text_role: Palette::ColorRole),
    [override] draw_item_pixmap(painter: &PainterType, rect: &RectType, alignment: i32, pixmap: &PixmapType),
    [override] size_from_contents(type: Style::ContentsType, option: &StyleOptionType, size: &SizeType, widget: &WidgetType) -> Size,
    [override] sub_element_rect(element: Style::SubElement, option: &StyleOptionType, widget: &WidgetType) -> Rect,
    [override] item_text_rect(fm: &FontMetricsType, r: &RectType, flags: i32, enabled: bool, text: String) -> Rect,
    [override] item_pixmap_rect(r: &RectType, flags: i32, pixmap: &PixmapType) -> Rect,
    [override] style_hint(hint: Style::StyleHint, option: &StyleOptionType, widget: &WidgetType, return_data: &StyleHintReturnType) -> i32,
    [override] layout_spacing(control1: SizePolicy::ControlType, control2: SizePolicy::ControlType, orientation: Rute::Orientation, option: &StyleOptionType, widget: &WidgetType) -> i32,
    [override] standard_icon(standard_icon: Style::StandardPixmap, option: &StyleOptionType, widget: &WidgetType) -> Icon,
    [override] standard_pixmap(standard_pixmap: Style::StandardPixmap, opt: &StyleOptionType, widget: &WidgetType) -> Pixmap,
    [override] generated_icon_pixmap(icon_mode: Icon::Mode, pixmap: &PixmapType, opt: &StyleOptionType) -> Pixmap,
    [override] standard_palette() -> Palette,
    [override] polish(widget: &WidgetType),
    [override] polish(pal: &PaletteType),
    [override] polish(app: &ApplicationType),
    [override] unpolish(widget: &WidgetType),
    [override] unpolish(app: &ApplicationType),
    [override] event(e: &EventType) -> bool,
}

