struct ProxyStyle : CommonStyle {
    base_style() -> Style?,
    set_base_style(style: *StyleType),
    [replace] draw_primitive(element: Style::PrimitiveElement, option: *StyleOptionType, painter: *PainterType, widget: *WidgetType),
    [replace] draw_item_text(painter: *PainterType, rect: &RectType, flags: i32, pal: &PaletteType, enabled: bool, text: String, text_role: Palette::ColorRole),
    [replace] draw_item_pixmap(painter: *PainterType, rect: &RectType, alignment: i32, pixmap: &PixmapType),
    [replace] size_from_contents(type: Style::ContentsType, option: *StyleOptionType, size: &SizeType, widget: *WidgetType) -> Size,
    [replace] sub_element_rect(element: Style::SubElement, option: *StyleOptionType, widget: *WidgetType) -> Rect,
    [replace] item_text_rect(fm: &FontMetricsType, r: &RectType, flags: i32, enabled: bool, text: String) -> Rect,
    [replace] item_pixmap_rect(r: &RectType, flags: i32, pixmap: &PixmapType) -> Rect,
    [replace] style_hint(hint: Style::StyleHint, option: *StyleOptionType, widget: *WidgetType, return_data: *StyleHintReturnType) -> i32,
    [replace] layout_spacing(control1: SizePolicy::ControlType, control2: SizePolicy::ControlType, orientation: Rute::Orientation, option: *StyleOptionType, widget: *WidgetType) -> i32,
    [replace] standard_icon(standard_icon: Style::StandardPixmap, option: *StyleOptionType, widget: *WidgetType) -> Icon,
    [replace] standard_pixmap(standard_pixmap: Style::StandardPixmap, opt: *StyleOptionType, widget: *WidgetType) -> Pixmap,
    [replace] generated_icon_pixmap(icon_mode: Icon::Mode, pixmap: &PixmapType, opt: *StyleOptionType) -> Pixmap,
    [replace] standard_palette() -> Palette,
    [replace] polish(widget: *WidgetType),
    [replace] polish(pal: &PaletteType),
    [replace] polish(app: *ApplicationType),
    [replace] unpolish(widget: *WidgetType),
    [replace] unpolish(app: *ApplicationType),
    [replace] event(e: *EventType) -> bool,
}

