struct ProgressDialog : Dialog {
    set_label(&LabelType label),
    set_cancel_button(&PushButtonType button),
    set_bar(&ProgressBarType bar),
    was_canceled() -> bool,
    minimum() -> i32,
    maximum() -> i32,
    value() -> i32,
    size_hint() -> SizeType,
    label_text() -> String,
    minimum_duration() -> i32,
    set_auto_reset(bool reset),
    auto_reset() -> bool,
    set_auto_close(bool close),
    auto_close() -> bool,
    open(&ObjectType receiver, &char member),
    [callback] cancel(),
    [callback] reset(),
    [callback] set_maximum(i32 maximum),
    [callback] set_minimum(i32 minimum),
    [callback] set_range(i32 minimum, i32 maximum),
    [callback] set_value(i32 progress),
    [callback] set_label_text(String text),
    [callback] set_cancel_button_text(String text),
    [callback] set_minimum_duration(i32 ms),
    canceled(),
    resize_event(&ResizeEventType event),
    close_event(&CloseEventType event),
    change_event(&EventType event),
    show_event(&ShowEventType event),
    force_show(),
}

