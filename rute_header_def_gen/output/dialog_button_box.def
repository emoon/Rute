struct DialogButtonBox : Widget {
    set_orientation(Rute::Orientation orientation),
    orientation() -> Rute::Orientation,
    add_button(&AbstractButtonType button, DialogButtonBox::ButtonRole role),
    add_button(String text, DialogButtonBox::ButtonRole role) -> &PushButtonType,
    add_button(DialogButtonBox::StandardButton button) -> &PushButtonType,
    remove_button(&AbstractButtonType button),
    clear(),
    buttons() -> <&AbstractButtonType>,
    button_role(&AbstractButtonType button) -> DialogButtonBox::ButtonRole,
    set_standard_buttons(StandardButtons buttons),
    standard_buttons() -> StandardButtons,
    standard_button(&AbstractButtonType button) -> DialogButtonBox::StandardButton,
    button(DialogButtonBox::StandardButton which) -> &PushButtonType,
    set_center_buttons(bool center),
    center_buttons() -> bool,
    [callback] clicked(&AbstractButtonType button),
    [callback] accepted(),
    [callback] help_requested(),
    [callback] rejected(),
    change_event(&EventType event),
    event(&EventType event) -> bool,
}

