struct TextCharFormat : TextFormat {
    is_valid() -> bool,
    set_font(&FontType font, TextCharFormat::FontPropertiesInheritanceBehavior behavior),
    set_font(&FontType font),
    font() -> FontType,
    set_font_family(String family),
    font_family() -> String,
    set_font_point_size(f32 size),
    font_point_size() -> f32,
    set_font_weight(i32 weight),
    font_weight() -> i32,
    set_font_italic(bool italic),
    font_italic() -> bool,
    set_font_capitalization(Font::Capitalization capitalization),
    font_capitalization() -> Font::Capitalization,
    set_font_letter_spacing_type(Font::SpacingType letter_spacing_type),
    font_letter_spacing_type() -> Font::SpacingType,
    set_font_letter_spacing(f32 spacing),
    font_letter_spacing() -> f32,
    set_font_word_spacing(f32 spacing),
    font_word_spacing() -> f32,
    set_font_underline(bool underline),
    font_underline() -> bool,
    set_font_overline(bool overline),
    font_overline() -> bool,
    set_underline_color(&ColorType color),
    underline_color() -> ColorType,
    set_font_fixed_pitch(bool fixed_pitch),
    font_fixed_pitch() -> bool,
    set_font_style_hint(Font::StyleHint hint, Font::StyleStrategy strategy),
    font_style_hint() -> Font::StyleHint,
    set_font_hinting_preference(Font::HintingPreference hinting_preference),
    font_hinting_preference() -> Font::HintingPreference,
    set_font_kerning(bool enable),
    font_kerning() -> bool,
    set_underline_style(TextCharFormat::UnderlineStyle style),
    underline_style() -> TextCharFormat::UnderlineStyle,
    set_vertical_alignment(TextCharFormat::VerticalAlignment alignment),
    vertical_alignment() -> TextCharFormat::VerticalAlignment,
    set_text_outline(&PenType pen),
    text_outline() -> PenType,
    set_tool_tip(String tip),
    tool_tip() -> String,
    set_anchor(bool anchor),
    is_anchor() -> bool,
    set_anchor_href(String value),
    anchor_href() -> String,
    set_anchor_name(String name),
    anchor_name() -> String,
    set_anchor_names(&StringListType names),
    anchor_names() -> StringListType,
    set_table_cell_row_span(i32 table_cell_row_span),
    table_cell_row_span() -> i32,
    set_table_cell_column_span(i32 table_cell_column_span),
    table_cell_column_span() -> i32,
}

