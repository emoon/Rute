struct WizardPage : Widget {
    set_title(String title),
    title() -> String,
    set_sub_title(String sub_title),
    sub_title() -> String,
    set_pixmap(Wizard::WizardPixmap which, &PixmapType pixmap),
    pixmap(Wizard::WizardPixmap which) -> PixmapType,
    set_final_page(bool final_page),
    is_final_page() -> bool,
    set_commit_page(bool commit_page),
    is_commit_page() -> bool,
    set_button_text(Wizard::WizardButton which, String text),
    button_text(Wizard::WizardButton which) -> String,
    initialize_page(),
    cleanup_page(),
    validate_page() -> bool,
    is_complete() -> bool,
    next_id() -> i32,
    [callback] complete_changed(),
    set_field(String name, &VariantType value),
    field(String name) -> VariantType,
    register_field(String name, &WidgetType widget, &char property, &char changed_signal),
    wizard() -> &WizardType,
}

