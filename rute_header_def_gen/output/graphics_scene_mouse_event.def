struct GraphicsSceneMouseEvent : GraphicsSceneEvent {
    pos() -> PointFType,
    set_pos(&PointFType pos),
    scene_pos() -> PointFType,
    set_scene_pos(&PointFType pos),
    screen_pos() -> PointType,
    set_screen_pos(&PointType pos),
    button_down_pos(Rute::MouseButton button) -> PointFType,
    set_button_down_pos(Rute::MouseButton button, &PointFType pos),
    button_down_scene_pos(Rute::MouseButton button) -> PointFType,
    set_button_down_scene_pos(Rute::MouseButton button, &PointFType pos),
    button_down_screen_pos(Rute::MouseButton button) -> PointType,
    set_button_down_screen_pos(Rute::MouseButton button, &PointType pos),
    last_pos() -> PointFType,
    set_last_pos(&PointFType pos),
    last_scene_pos() -> PointFType,
    set_last_scene_pos(&PointFType pos),
    last_screen_pos() -> PointType,
    set_last_screen_pos(&PointType pos),
    buttons() -> Rute::MouseButtons,
    set_buttons(Rute::MouseButtons buttons),
    button() -> Rute::MouseButton,
    set_button(Rute::MouseButton button),
    modifiers() -> Rute::KeyboardModifiers,
    set_modifiers(Rute::KeyboardModifiers modifiers),
    source() -> Rute::MouseEventSource,
    set_source(Rute::MouseEventSource source),
    flags() -> Rute::MouseEventFlags,
    set_flags(Rute::MouseEventFlags arg0),
}

