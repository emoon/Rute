
struct MessageLogger {
    debug(&char msg),
    no_debug(&char arg0),
    info(&char msg),
    warning(&char msg),
    critical(&char msg),
    debug(&LoggingCategoryType cat, &char msg),
    debug(CategoryFunction cat_func, &char msg),
    info(&LoggingCategoryType cat, &char msg),
    info(CategoryFunction cat_func, &char msg),
    warning(&LoggingCategoryType cat, &char msg),
    warning(CategoryFunction cat_func, &char msg),
    critical(&LoggingCategoryType cat, &char msg),
    critical(CategoryFunction cat_func, &char msg),
    fatal(&char msg),
    debug() -> DebugType,
    debug(&LoggingCategoryType cat) -> DebugType,
    debug(CategoryFunction cat_func) -> DebugType,
    info() -> DebugType,
    info(&LoggingCategoryType cat) -> DebugType,
    info(CategoryFunction cat_func) -> DebugType,
    warning() -> DebugType,
    warning(&LoggingCategoryType cat) -> DebugType,
    warning(CategoryFunction cat_func) -> DebugType,
    critical() -> DebugType,
    critical(&LoggingCategoryType cat) -> DebugType,
    critical(CategoryFunction cat_func) -> DebugType,
    no_debug() -> NoDebugType,
}

