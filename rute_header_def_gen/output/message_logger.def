
struct MessageLogger {
    debug(msg: &char),
    no_debug(arg0: &char),
    info(msg: &char),
    warning(msg: &char),
    critical(msg: &char),
    debug(cat: &LoggingCategoryType, msg: &char),
    debug(cat_func: CategoryFunction, msg: &char),
    info(cat: &LoggingCategoryType, msg: &char),
    info(cat_func: CategoryFunction, msg: &char),
    warning(cat: &LoggingCategoryType, msg: &char),
    warning(cat_func: CategoryFunction, msg: &char),
    critical(cat: &LoggingCategoryType, msg: &char),
    critical(cat_func: CategoryFunction, msg: &char),
    fatal(msg: &char),
    debug() -> Debug,
    debug(cat: &LoggingCategoryType) -> Debug,
    debug(cat_func: CategoryFunction) -> Debug,
    info() -> Debug,
    info(cat: &LoggingCategoryType) -> Debug,
    info(cat_func: CategoryFunction) -> Debug,
    warning() -> Debug,
    warning(cat: &LoggingCategoryType) -> Debug,
    warning(cat_func: CategoryFunction) -> Debug,
    critical() -> Debug,
    critical(cat: &LoggingCategoryType) -> Debug,
    critical(cat_func: CategoryFunction) -> Debug,
    no_debug() -> NoDebug,
}

