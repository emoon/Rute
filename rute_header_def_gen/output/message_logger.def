
struct MessageLogger {
    debug(msg: &char),
    no_debug(arg0: &char),
    info(msg: &char),
    warning(msg: &char),
    critical(msg: &char),
    debug(cat: &LoggingCategoryType, msg: &char),
    debug(cat_func: CategoryFunction, msg: &char),
    info(cat: &LoggingCategoryType, msg: &char),
    info(cat_func: CategoryFunction, msg: &char),
    warning(cat: &LoggingCategoryType, msg: &char),
    warning(cat_func: CategoryFunction, msg: &char),
    critical(cat: &LoggingCategoryType, msg: &char),
    critical(cat_func: CategoryFunction, msg: &char),
    fatal(msg: &char),
    debug() -> DebugType,
    debug(cat: &LoggingCategoryType) -> DebugType,
    debug(cat_func: CategoryFunction) -> DebugType,
    info() -> DebugType,
    info(cat: &LoggingCategoryType) -> DebugType,
    info(cat_func: CategoryFunction) -> DebugType,
    warning() -> DebugType,
    warning(cat: &LoggingCategoryType) -> DebugType,
    warning(cat_func: CategoryFunction) -> DebugType,
    critical() -> DebugType,
    critical(cat: &LoggingCategoryType) -> DebugType,
    critical(cat_func: CategoryFunction) -> DebugType,
    no_debug() -> NoDebugType,
}

