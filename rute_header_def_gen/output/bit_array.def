
struct BitArray {
    swap(&BitArrayType other),
    size() -> i32,
    count() -> i32,
    count(bool on) -> i32,
    is_empty() -> bool,
    is_null() -> bool,
    resize(i32 size),
    detach(),
    is_detached() -> bool,
    clear(),
    test_bit(i32 i) -> bool,
    set_bit(i32 i),
    set_bit(i32 i, bool val),
    clear_bit(i32 i),
    toggle_bit(i32 i) -> bool,
    at(i32 i) -> bool,
    fill(bool val, i32 size) -> bool,
    fill(bool val, i32 first, i32 last),
}

