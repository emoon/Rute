struct InputDialog : Dialog {
    set_input_mode(InputDialog::InputMode mode),
    input_mode() -> InputDialog::InputMode,
    set_label_text(String text),
    label_text() -> String,
    set_option(InputDialog::InputDialogOption option, bool on),
    test_option(InputDialog::InputDialogOption option) -> bool,
    set_options(InputDialogOptions options),
    options() -> InputDialogOptions,
    set_text_value(String text),
    text_value() -> String,
    set_text_echo_mode(LineEdit::EchoMode mode),
    text_echo_mode() -> LineEdit::EchoMode,
    set_combo_box_editable(bool editable),
    is_combo_box_editable() -> bool,
    set_combo_box_items(&StringListType items),
    combo_box_items() -> StringListType,
    set_int_value(i32 value),
    int_value() -> i32,
    set_int_minimum(i32 min),
    int_minimum() -> i32,
    set_int_maximum(i32 max),
    int_maximum() -> i32,
    set_int_range(i32 min, i32 max),
    set_int_step(i32 step),
    int_step() -> i32,
    set_double_value(double value),
    double_value() -> double,
    set_double_minimum(double min),
    double_minimum() -> double,
    set_double_maximum(double max),
    double_maximum() -> double,
    set_double_range(double min, double max),
    set_double_decimals(i32 decimals),
    double_decimals() -> i32,
    set_ok_button_text(String text),
    ok_button_text() -> String,
    set_cancel_button_text(String text),
    cancel_button_text() -> String,
    open(&ObjectType receiver, &char member),
    minimum_size_hint() -> SizeType,
    size_hint() -> SizeType,
    set_visible(bool visible),
    get_text(&WidgetType parent, String title, String label, LineEdit::EchoMode echo, String text, &bool ok, Rute::WindowFlags flags, Rute::InputMethodHints input_method_hints) -> String,
    get_multi_line_text(&WidgetType parent, String title, String label, String text, &bool ok, Rute::WindowFlags flags, Rute::InputMethodHints input_method_hints) -> String,
    get_item(&WidgetType parent, String title, String label, &StringListType items, i32 current, bool editable, &bool ok, Rute::WindowFlags flags, Rute::InputMethodHints input_method_hints) -> String,
    get_int(&WidgetType parent, String title, String label, i32 value, i32 min_value, i32 max_value, i32 step, &bool ok, Rute::WindowFlags flags) -> i32,
    get_double(&WidgetType parent, String title, String label, double value, double min_value, double max_value, i32 decimals, &bool ok, Rute::WindowFlags flags) -> double,
    get_double(&WidgetType parent, String title, String label, double value, double min_value, double max_value, i32 decimals, &bool ok, Rute::WindowFlags flags, double step) -> double,
    set_double_step(double step),
    double_step() -> double,
    [callback] text_value_changed(String text),
    [callback] text_value_selected(String text),
    [callback] int_value_changed(i32 value),
    [callback] int_value_selected(i32 value),
    [callback] double_value_changed(double value),
    [callback] double_value_selected(double value),
    done(i32 result),
}

