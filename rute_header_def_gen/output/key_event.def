struct KeyEvent : InputEvent {
    key() -> i32,
    matches(key: KeySequence::StandardKey) -> bool,
    modifiers() -> Rute::KeyboardModifiers,
    text() -> String,
    is_auto_repeat() -> bool,
    count() -> i32,
    native_scan_code() -> u32,
    native_virtual_key() -> u32,
    native_modifiers() -> u32,
}

