struct ScrollPrepareEvent : Event {
    start_pos() -> PointFType,
    viewport_size() -> SizeFType,
    content_pos_range() -> RectFType,
    content_pos() -> PointFType,
    set_viewport_size(&SizeFType size),
    set_content_pos_range(&RectFType rect),
    set_content_pos(&PointFType pos),
}

