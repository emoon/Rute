
struct MainWindowLayoutState {
    apply(bool animated),
    delete_all_layout_items(),
    size_hint() -> SizeType,
    minimum_size() -> SizeType,
    fit_layout(),
    item_at(i32 index, &i32 x) -> &LayoutItemType,
    take_at(i32 index, &i32 x) -> &LayoutItemType,
    index_of(&WidgetType widget) -> <i32>,
    item(&List<int>Type path) -> &LayoutItemType,
    item_rect(&List<int>Type path) -> RectType,
    gap_rect(&List<int>Type path) -> RectType,
    contains(&WidgetType widget) -> bool,
    gap_index(&WidgetType widget, &PointType pos) -> <i32>,
    insert_gap(&List<int>Type path, &LayoutItemType item) -> bool,
    remove(&List<int>Type path),
    remove(&LayoutItemType item),
    clear(),
    is_valid() -> bool,
    plug(&List<int>Type path) -> &LayoutItemType,
    unplug(&List<int>Type path, &MainWindowLayoutStateType saved_state) -> &LayoutItemType,
    save_state(&DataStreamType stream),
    check_format(&DataStreamType stream) -> bool,
    restore_state(&DataStreamType stream, &MainWindowLayoutStateType old_state) -> bool,
}

