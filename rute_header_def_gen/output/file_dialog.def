struct FileDialog : Dialog {
    set_directory(String directory),
    set_directory(&DirType directory),
    directory() -> DirType,
    set_directory_url(&UrlType directory),
    directory_url() -> UrlType,
    select_file(String filename),
    selected_files() -> StringListType,
    select_url(&UrlType url),
    selected_urls() -> <UrlType>,
    set_name_filter_details_visible(bool enabled),
    is_name_filter_details_visible() -> bool,
    set_name_filter(String filter),
    set_name_filters(&StringListType filters),
    name_filters() -> StringListType,
    select_name_filter(String filter),
    selected_mime_type_filter() -> String,
    selected_name_filter() -> String,
    set_mime_type_filters(&StringListType filters),
    mime_type_filters() -> StringListType,
    select_mime_type_filter(String filter),
    filter() -> Dir::Filters,
    set_filter(Dir::Filters filters),
    set_view_mode(FileDialog::ViewMode mode),
    view_mode() -> FileDialog::ViewMode,
    set_file_mode(FileDialog::FileMode mode),
    file_mode() -> FileDialog::FileMode,
    set_accept_mode(FileDialog::AcceptMode mode),
    accept_mode() -> FileDialog::AcceptMode,
    set_read_only(bool enabled),
    is_read_only() -> bool,
    set_resolve_symlinks(bool enabled),
    resolve_symlinks() -> bool,
    set_sidebar_urls(&List<QUrl>Type urls),
    sidebar_urls() -> <UrlType>,
    save_state() -> ByteArrayType,
    restore_state(&ByteArrayType state) -> bool,
    set_confirm_overwrite(bool enabled),
    confirm_overwrite() -> bool,
    set_default_suffix(String suffix),
    default_suffix() -> String,
    set_history(&StringListType paths),
    history() -> StringListType,
    set_item_delegate(&AbstractItemDelegateType delegate),
    item_delegate() -> &AbstractItemDelegateType,
    set_icon_provider(&FileIconProviderType provider),
    icon_provider() -> &FileIconProviderType,
    set_label_text(FileDialog::DialogLabel label, String text),
    label_text(FileDialog::DialogLabel label) -> String,
    set_supported_schemes(&StringListType schemes),
    supported_schemes() -> StringListType,
    set_proxy_model(&AbstractProxyModelType model),
    proxy_model() -> &AbstractProxyModelType,
    set_option(FileDialog::Option option, bool on),
    test_option(FileDialog::Option option) -> bool,
    set_options(Options options),
    options() -> Options,
    open(&ObjectType receiver, &char member),
    set_visible(bool visible),
    [callback] file_selected(String file),
    [callback] files_selected(&StringListType files),
    [callback] current_changed(String path),
    [callback] directory_entered(String directory),
    [callback] url_selected(&UrlType url),
    [callback] urls_selected(&List<QUrl>Type urls),
    [callback] current_url_changed(&UrlType url),
    [callback] directory_url_entered(&UrlType directory),
    [callback] filter_selected(String filter),
    get_open_file_name(&WidgetType parent, String caption, String dir, String filter, String selected_filter, Options options) -> String,
    get_open_file_url(&WidgetType parent, String caption, &UrlType dir, String filter, String selected_filter, Options options, &StringListType supported_schemes) -> UrlType,
    get_save_file_name(&WidgetType parent, String caption, String dir, String filter, String selected_filter, Options options) -> String,
    get_save_file_url(&WidgetType parent, String caption, &UrlType dir, String filter, String selected_filter, Options options, &StringListType supported_schemes) -> UrlType,
    get_existing_directory(&WidgetType parent, String caption, String dir, Options options) -> String,
    get_existing_directory_url(&WidgetType parent, String caption, &UrlType dir, Options options, &StringListType supported_schemes) -> UrlType,
    get_open_file_names(&WidgetType parent, String caption, String dir, String filter, String selected_filter, Options options) -> StringListType,
    get_open_file_urls(&WidgetType parent, String caption, &UrlType dir, String filter, String selected_filter, Options options, &StringListType supported_schemes) -> <UrlType>,
    done(i32 result),
    accept(),
    change_event(&EventType e),
}

