struct DesktopWidget : Widget {
    screen_number(widget: *WidgetType) -> i32,
    is_virtual_desktop() -> bool,
    num_screens() -> i32,
    screen_count() -> i32,
    primary_screen() -> i32,
    screen_number(arg0: &PointType) -> i32,
    screen(screen: i32) -> Widget?,
    [signal] resized(arg0: i32),
    [signal] work_area_resized(arg0: i32),
    [signal] screen_count_changed(arg0: i32),
    [signal] primary_screen_changed(),
    [event] resize_event(e: *ResizeEventType),
}

