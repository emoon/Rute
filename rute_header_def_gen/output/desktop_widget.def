struct DesktopWidget : Widget {
    is_virtual_desktop() -> bool,
    num_screens() -> i32,
    screen_count() -> i32,
    primary_screen() -> i32,
    screen_number(widget: &WidgetType) -> i32,
    screen_number(arg0: &PointType) -> i32,
    screen(screen: i32) -> &WidgetType,
    [callback] resized(arg0: i32),
    [callback] work_area_resized(arg0: i32),
    [callback] screen_count_changed(arg0: i32),
    [callback] primary_screen_changed(),
    resize_event(e: &ResizeEventType),
}

