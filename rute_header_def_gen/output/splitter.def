struct Splitter : Frame {
    add_widget(&WidgetType widget),
    insert_widget(i32 index, &WidgetType widget),
    replace_widget(i32 index, &WidgetType widget) -> &WidgetType,
    set_orientation(Rute::Orientation arg0),
    orientation() -> Rute::Orientation,
    set_children_collapsible(bool arg0),
    children_collapsible() -> bool,
    set_collapsible(i32 index, bool arg0),
    is_collapsible(i32 index) -> bool,
    set_opaque_resize(bool opaque),
    opaque_resize() -> bool,
    refresh(),
    size_hint() -> SizeType,
    minimum_size_hint() -> SizeType,
    sizes() -> <i32>,
    set_sizes(<i32> list),
    save_state() -> ByteArrayType,
    restore_state(&ByteArrayType state) -> bool,
    handle_width() -> i32,
    set_handle_width(i32 arg0),
    index_of(&WidgetType w) -> i32,
    widget(i32 index) -> &WidgetType,
    count() -> i32,
    get_range(i32 index, &i32 arg0, &i32 arg1),
    handle(i32 index) -> &SplitterHandleType,
    [callback] splitter_moved(i32 pos, i32 index),
    create_handle() -> &SplitterHandleType,
    child_event(&ChildEventType arg0),
    event(&EventType arg0) -> bool,
    resize_event(&ResizeEventType arg0),
    change_event(&EventType arg0),
    move_splitter(i32 pos, i32 index),
    set_rubber_band(i32 position),
    closest_legal_position(i32 arg0, i32 arg1) -> i32,
}

