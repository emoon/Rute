
struct ListWidgetItem {
    clone() -> &ListWidgetItemType,
    list_widget() -> &ListWidgetType,
    set_selected(bool select),
    is_selected() -> bool,
    set_hidden(bool hide),
    is_hidden() -> bool,
    flags() -> Rute::ItemFlags,
    set_flags(Rute::ItemFlags flags),
    text() -> String,
    set_text(String text),
    icon() -> IconType,
    set_icon(&IconType icon),
    status_tip() -> String,
    set_status_tip(String status_tip),
    tool_tip() -> String,
    set_tool_tip(String tool_tip),
    whats_this() -> String,
    set_whats_this(String whats_this),
    font() -> FontType,
    set_font(&FontType font),
    text_alignment() -> i32,
    set_text_alignment(i32 alignment),
    background_color() -> ColorType,
    set_background_color(&ColorType color),
    background() -> BrushType,
    set_background(&BrushType brush),
    text_color() -> ColorType,
    set_text_color(&ColorType color),
    foreground() -> BrushType,
    set_foreground(&BrushType brush),
    check_state() -> Rute::CheckState,
    set_check_state(Rute::CheckState state),
    size_hint() -> SizeType,
    set_size_hint(&SizeType size),
    data(i32 role) -> VariantType,
    set_data(i32 role, &VariantType value),
    read(&DataStreamType in),
    write(&DataStreamType out),
    type() -> i32,
}

