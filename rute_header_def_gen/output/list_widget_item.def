enum ItemType {
    Type,
    UserType,
}


struct ListWidgetItem {
    [override] clone() -> &ListWidgetItemType,
    list_widget() -> &ListWidgetType,
    set_selected(select: bool),
    is_selected() -> bool,
    set_hidden(hide: bool),
    is_hidden() -> bool,
    flags() -> Rute::ItemFlags,
    set_flags(flags: Rute::ItemFlags),
    text() -> String,
    set_text(text: String),
    icon() -> IconType,
    set_icon(icon: &IconType),
    status_tip() -> String,
    set_status_tip(status_tip: String),
    tool_tip() -> String,
    set_tool_tip(tool_tip: String),
    whats_this() -> String,
    set_whats_this(whats_this: String),
    font() -> FontType,
    set_font(font: &FontType),
    text_alignment() -> i32,
    set_text_alignment(alignment: i32),
    background_color() -> ColorType,
    [override] set_background_color(color: &ColorType),
    background() -> BrushType,
    set_background(brush: &BrushType),
    text_color() -> ColorType,
    set_text_color(color: &ColorType),
    foreground() -> BrushType,
    set_foreground(brush: &BrushType),
    check_state() -> Rute::CheckState,
    set_check_state(state: Rute::CheckState),
    size_hint() -> SizeType,
    set_size_hint(size: &SizeType),
    [override] data(role: i32) -> VariantType,
    [override] set_data(role: i32, value: &VariantType),
    [override] read(in: &DataStreamType),
    [override] write(out: &DataStreamType),
    type() -> i32,
}

