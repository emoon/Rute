enum Roles {
    FileIconRole,
    FilePathRole,
    FileNameRole,
    FilePermissions,
}

struct FileSystemModel : AbstractItemModel {
    [event] root_path_changed(new_path: String),
    [event] file_renamed(path: String, old_name: String, new_name: String),
    [event] directory_loaded(path: String),
    [override] index(row: i32, column: i32, parent: &ModelIndexType) -> ModelIndex,
    index(path: String, column: i32) -> ModelIndex,
    [override] parent(child: &ModelIndexType) -> ModelIndex,
    [override] sibling(row: i32, column: i32, idx: &ModelIndexType) -> ModelIndex,
    [override] has_children(parent: &ModelIndexType) -> bool,
    [override] can_fetch_more(parent: &ModelIndexType) -> bool,
    [override] fetch_more(parent: &ModelIndexType),
    [override] row_count(parent: &ModelIndexType) -> i32,
    [override] column_count(parent: &ModelIndexType) -> i32,
    my_computer(role: i32) -> Variant,
    [override] data(index: &ModelIndexType, role: i32) -> Variant,
    [override] set_data(index: &ModelIndexType, value: &VariantType, role: i32) -> bool,
    [override] header_data(section: i32, orientation: Rute::Orientation, role: i32) -> Variant,
    [override] flags(index: &ModelIndexType) -> Rute::ItemFlags,
    [override] sort(column: i32, order: Rute::SortOrder),
    [override] mime_types() -> StringList,
    [override] mime_data(indexes: &ModelIndexListType) -> MimeData?,
    [override] drop_mime_data(data: &MimeDataType, action: Rute::DropAction, row: i32, column: i32, parent: &ModelIndexType) -> bool,
    [override] supported_drop_actions() -> Rute::DropActions,
    set_root_path(path: String) -> ModelIndex,
    root_path() -> String,
    root_directory() -> Dir,
    set_icon_provider(provider: &FileIconProviderType),
    icon_provider() -> FileIconProvider?,
    set_filter(filters: Dir::Filters),
    filter() -> Dir::Filters,
    set_resolve_symlinks(enable: bool),
    resolve_symlinks() -> bool,
    set_read_only(enable: bool),
    is_read_only() -> bool,
    set_name_filter_disables(enable: bool),
    name_filter_disables() -> bool,
    set_name_filters(filters: &StringListType),
    name_filters() -> StringList,
    file_path(index: &ModelIndexType) -> String,
    is_dir(index: &ModelIndexType) -> bool,
    size(index: &ModelIndexType) -> i64,
    type(index: &ModelIndexType) -> String,
    last_modified(index: &ModelIndexType) -> DateTime,
    mkdir(parent: &ModelIndexType, name: String) -> ModelIndex,
    rmdir(index: &ModelIndexType) -> bool,
    file_name(index: &ModelIndexType) -> String,
    file_icon(index: &ModelIndexType) -> Icon,
    permissions(index: &ModelIndexType) -> File::Permissions,
    file_info(index: &ModelIndexType) -> FileInfo,
    remove(index: &ModelIndexType) -> bool,
    [override] timer_event(event: &TimerEventType),
    [override] event(event: &EventType) -> bool,
}

