struct DoubleSpinBox : AbstractSpinBox {
    value() -> double,
    prefix() -> String,
    set_prefix(prefix: String),
    suffix() -> String,
    set_suffix(suffix: String),
    clean_text() -> String,
    single_step() -> double,
    set_single_step(val: double),
    minimum() -> double,
    set_minimum(min: double),
    maximum() -> double,
    set_maximum(max: double),
    set_range(min: double, max: double),
    decimals() -> i32,
    set_decimals(prec: i32),
    [override] validate(input: String, pos: &i32) -> Validator::State,
    [override] value_from_text(text: String) -> double,
    [override] text_from_value(val: double) -> String,
    [override] fixup(str: String),
    set_value(val: double),
    [event] value_changed(arg0: double),
    [event] value_changed(arg0: String),
}

