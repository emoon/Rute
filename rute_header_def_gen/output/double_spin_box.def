struct DoubleSpinBox : AbstractSpinBox {
    value() -> double,
    prefix() -> String,
    set_prefix(String prefix),
    suffix() -> String,
    set_suffix(String suffix),
    clean_text() -> String,
    single_step() -> double,
    set_single_step(double val),
    minimum() -> double,
    set_minimum(double min),
    maximum() -> double,
    set_maximum(double max),
    set_range(double min, double max),
    decimals() -> i32,
    set_decimals(i32 prec),
    validate(String input, &i32 pos) -> Validator::State,
    value_from_text(String text) -> double,
    text_from_value(double val) -> String,
    fixup(String str),
    set_value(double val),
    [callback] value_changed(double arg0),
    [callback] value_changed(String arg0),
}

