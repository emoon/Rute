struct ColumnView : AbstractItemView {
    [callback] update_preview_widget(index: &ModelIndexType),
    index_at(point: &PointType) -> ModelIndexType,
    scroll_to(index: &ModelIndexType, hint: AbstractItemView::ScrollHint),
    size_hint() -> SizeType,
    visual_rect(index: &ModelIndexType) -> RectType,
    set_model(model: &AbstractItemModelType),
    set_selection_model(selection_model: &ItemSelectionModelType),
    set_root_index(index: &ModelIndexType),
    select_all(),
    set_resize_grips_visible(visible: bool),
    resize_grips_visible() -> bool,
    preview_widget() -> &WidgetType,
    set_preview_widget(widget: &WidgetType),
    set_column_widths(list: [i32]),
    column_widths() -> [i32],
    is_index_hidden(index: &ModelIndexType) -> bool,
    move_cursor(cursor_action: AbstractItemView::CursorAction, modifiers: Rute::KeyboardModifiers) -> ModelIndexType,
    resize_event(event: &ResizeEventType),
    set_selection(rect: &RectType, command: ItemSelectionModel::SelectionFlags),
    visual_region_for_selection(selection: &ItemSelectionType) -> RegionType,
    horizontal_offset() -> i32,
    vertical_offset() -> i32,
    rows_inserted(parent: &ModelIndexType, start: i32, end: i32),
    current_changed(current: &ModelIndexType, previous: &ModelIndexType),
    scroll_contents_by(dx: i32, dy: i32),
    create_column(root_index: &ModelIndexType) -> &AbstractItemViewType,
    initialize_column(column: &AbstractItemViewType),
}

