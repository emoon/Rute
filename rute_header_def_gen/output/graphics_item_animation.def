struct GraphicsItemAnimation : Object {
    item() -> &GraphicsItemType,
    set_item(&GraphicsItemType item),
    time_line() -> &TimeLineType,
    set_time_line(&TimeLineType time_line),
    pos_at(f32 step) -> PointFType,
    pos_list() -> <Pair<qreal,Type>,
    set_pos_at(f32 step, &PointFType pos),
    rotation_at(f32 step) -> f32,
    rotation_list() -> <Pair<qreal,Type>,
    set_rotation_at(f32 step, f32 angle),
    x_translation_at(f32 step) -> f32,
    y_translation_at(f32 step) -> f32,
    set_translation_at(f32 step, f32 dx, f32 dy),
    vertical_scale_at(f32 step) -> f32,
    horizontal_scale_at(f32 step) -> f32,
    scale_list() -> <Pair<qreal,Type>,
    set_scale_at(f32 step, f32 sx, f32 sy),
    vertical_shear_at(f32 step) -> f32,
    horizontal_shear_at(f32 step) -> f32,
    shear_list() -> <Pair<qreal,Type>,
    set_shear_at(f32 step, f32 sh, f32 sv),
    clear(),
    set_step(f32 x),
    reset(),
    before_animation_step(f32 step),
    after_animation_step(f32 step),
}

