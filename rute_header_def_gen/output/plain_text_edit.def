    input_method_query(query: Rute::InputMethodQuery, argument: VariantType) -> VariantType,
    set_plain_text(text: String),
    cut(),
    copy(),
    paste(),
    undo(),
    redo(),
    clear(),
    select_all(),
    insert_plain_text(text: String),
    append_plain_text(text: String),
    append_html(html: String),
    center_cursor(),
    zoom_in(range: i32),
    zoom_out(range: i32),
    [callback] text_changed(),
    [callback] undo_available(b: bool),
    [callback] redo_available(b: bool),
    [callback] copy_available(b: bool),
    [callback] selection_changed(),
    [callback] cursor_position_changed(),
    [callback] update_request(rect: &RectType, dy: i32),
    [callback] block_count_changed(new_block_count: i32),
    [callback] modification_changed(arg0: bool),
    event(e: &EventType) -> bool,
    timer_event(e: &TimerEventType),
    key_press_event(e: &KeyEventType),
    key_release_event(e: &KeyEventType),
    resize_event(e: &ResizeEventType),
    paint_event(e: &PaintEventType),
    mouse_press_event(e: &MouseEventType),
    mouse_move_event(e: &MouseEventType),
    mouse_release_event(e: &MouseEventType),
    mouse_double_click_event(e: &MouseEventType),
    focus_next_prev_child(next: bool) -> bool,
    context_menu_event(e: &ContextMenuEventType),
    drag_enter_event(e: &DragEnterEventType),
    drag_leave_event(e: &DragLeaveEventType),
    drag_move_event(e: &DragMoveEventType),
    drop_event(e: &DropEventType),
    focus_in_event(e: &FocusEventType),
    focus_out_event(e: &FocusEventType),
    show_event(arg0: &ShowEventType),
    change_event(e: &EventType),
    wheel_event(e: &WheelEventType),
    create_mime_data_from_selection() -> &MimeDataType,
    can_insert_from_mime_data(source: &MimeDataType) -> bool,
    insert_from_mime_data(source: &MimeDataType),
    input_method_event(arg0: &InputMethodEventType),
    scroll_contents_by(dx: i32, dy: i32),
    do_set_text_cursor(cursor: &TextCursorType),
    first_visible_block() -> TextBlockType,
    content_offset() -> PointFType,
    block_bounding_rect(block: &TextBlockType) -> RectFType,
    get_paint_context() -> AbstractTextDocumentLayout::PaintContext,
    zoom_in_f(range: f32),
