[org_name(QValidator)]
enum State {
    Invalid,
    Intermediate,
    Acceptable,
}

struct Validator : Object {
    set_locale(locale: &LocaleType),
    locale() -> Locale,
    [event] validate(arg0: String, arg1: &i32) -> Validator::State,
    [event] fixup(arg0: String),
    [signal] changed(),
}

