enum ResultFlag {
    Ignore,
    MayBeGesture,
    TriggerGesture,
    FinishGesture,
    CancelGesture,
    ResultState_Mask,
    ConsumeEventHint,
    ResultHint_Mask,
}


struct GestureRecognizer {
    [override] create(target: &ObjectType) -> &GestureType,
    [override] recognize(state: &GestureType, watched: &ObjectType, event: &EventType) -> Result,
    [override] reset(state: &GestureType),
    [static] register_recognizer(recognizer: &GestureRecognizerType) -> Rute::GestureType,
    [static] unregister_recognizer(type: Rute::GestureType),
}

