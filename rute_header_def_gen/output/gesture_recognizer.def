
struct GestureRecognizer {
    create(target: &ObjectType) -> &GestureType,
    recognize(state: &GestureType, watched: &ObjectType, event: &EventType) -> Result,
    reset(state: &GestureType),
    register_recognizer(recognizer: &GestureRecognizerType) -> Rute::GestureType,
    unregister_recognizer(type: Rute::GestureType),
}

