
struct Vector2D {
    is_null() -> bool,
    x() -> f32,
    y() -> f32,
    set_x(x: f32),
    set_y(y: f32),
    length() -> f32,
    length_squared() -> f32,
    normalized() -> Vector2DType,
    normalize(),
    distance_to_point(point: &Vector2DType) -> f32,
    distance_to_line(point: &Vector2DType, direction: &Vector2DType) -> f32,
    dot_product(v1: &Vector2DType, v2: &Vector2DType) -> f32,
    to_vector3_d() -> Vector3DType,
    to_vector4_d() -> Vector4DType,
    to_point() -> PointType,
    to_point_f() -> PointFType,
}

