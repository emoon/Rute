struct ToolBox : Frame {
    add_item(&WidgetType widget, String text) -> i32,
    add_item(&WidgetType widget, &IconType icon, String text) -> i32,
    insert_item(i32 index, &WidgetType widget, String text) -> i32,
    insert_item(i32 index, &WidgetType widget, &IconType icon, String text) -> i32,
    remove_item(i32 index),
    set_item_enabled(i32 index, bool enabled),
    is_item_enabled(i32 index) -> bool,
    set_item_text(i32 index, String text),
    item_text(i32 index) -> String,
    set_item_icon(i32 index, &IconType icon),
    item_icon(i32 index) -> IconType,
    set_item_tool_tip(i32 index, String tool_tip),
    item_tool_tip(i32 index) -> String,
    current_index() -> i32,
    current_widget() -> &WidgetType,
    widget(i32 index) -> &WidgetType,
    index_of(&WidgetType widget) -> i32,
    count() -> i32,
    set_current_index(i32 index),
    set_current_widget(&WidgetType widget),
    [callback] current_changed(i32 index),
    event(&EventType e) -> bool,
    item_inserted(i32 index),
    item_removed(i32 index),
    show_event(&ShowEventType e),
    change_event(&EventType arg0),
}

