struct TableWidget : TableView {
    set_row_count(i32 rows),
    row_count() -> i32,
    set_column_count(i32 columns),
    column_count() -> i32,
    row(&TableWidgetItemType item) -> i32,
    column(&TableWidgetItemType item) -> i32,
    item(i32 row, i32 column) -> &TableWidgetItemType,
    set_item(i32 row, i32 column, &TableWidgetItemType item),
    take_item(i32 row, i32 column) -> &TableWidgetItemType,
    vertical_header_item(i32 row) -> &TableWidgetItemType,
    set_vertical_header_item(i32 row, &TableWidgetItemType item),
    take_vertical_header_item(i32 row) -> &TableWidgetItemType,
    horizontal_header_item(i32 column) -> &TableWidgetItemType,
    set_horizontal_header_item(i32 column, &TableWidgetItemType item),
    take_horizontal_header_item(i32 column) -> &TableWidgetItemType,
    set_vertical_header_labels(&StringListType labels),
    set_horizontal_header_labels(&StringListType labels),
    current_row() -> i32,
    current_column() -> i32,
    current_item() -> &TableWidgetItemType,
    set_current_item(&TableWidgetItemType item),
    set_current_item(&TableWidgetItemType item, ItemSelectionModel::SelectionFlags command),
    set_current_cell(i32 row, i32 column),
    set_current_cell(i32 row, i32 column, ItemSelectionModel::SelectionFlags command),
    sort_items(i32 column, Rute::SortOrder order),
    set_sorting_enabled(bool enable),
    is_sorting_enabled() -> bool,
    edit_item(&TableWidgetItemType item),
    open_persistent_editor(&TableWidgetItemType item),
    close_persistent_editor(&TableWidgetItemType item),
    is_persistent_editor_open(&TableWidgetItemType item) -> bool,
    cell_widget(i32 row, i32 column) -> &WidgetType,
    set_cell_widget(i32 row, i32 column, &WidgetType widget),
    remove_cell_widget(i32 row, i32 column),
    is_item_selected(&TableWidgetItemType item) -> bool,
    set_item_selected(&TableWidgetItemType item, bool select),
    set_range_selected(&TableWidgetSelectionRangeType range, bool select),
    selected_ranges() -> <TableWidgetSelectionRangeType>,
    selected_items() -> <&TableWidgetItemType>,
    find_items(String text, Rute::MatchFlags flags) -> <&TableWidgetItemType>,
    visual_row(i32 logical_row) -> i32,
    visual_column(i32 logical_column) -> i32,
    item_at(&PointType p) -> &TableWidgetItemType,
    item_at(i32 x, i32 y) -> &TableWidgetItemType,
    visual_item_rect(&TableWidgetItemType item) -> RectType,
    item_prototype() -> &TableWidgetItemType,
    set_item_prototype(&TableWidgetItemType item),
    scroll_to_item(&TableWidgetItemType item, AbstractItemView::ScrollHint hint),
    insert_row(i32 row),
    insert_column(i32 column),
    remove_row(i32 row),
    remove_column(i32 column),
    clear(),
    clear_contents(),
    item_pressed(&TableWidgetItemType item),
    item_clicked(&TableWidgetItemType item),
    item_double_clicked(&TableWidgetItemType item),
    item_activated(&TableWidgetItemType item),
    item_entered(&TableWidgetItemType item),
    item_changed(&TableWidgetItemType item),
    current_item_changed(&TableWidgetItemType current, &TableWidgetItemType previous),
    item_selection_changed(),
    cell_pressed(i32 row, i32 column),
    cell_clicked(i32 row, i32 column),
    cell_double_clicked(i32 row, i32 column),
    cell_activated(i32 row, i32 column),
    cell_entered(i32 row, i32 column),
    cell_changed(i32 row, i32 column),
    current_cell_changed(i32 current_row, i32 current_column, i32 previous_row, i32 previous_column),
    event(&EventType e) -> bool,
    mime_types() -> StringListType,
    mime_data(&List<QTableWidgetItemType items) -> &MimeDataType,
    drop_mime_data(i32 row, i32 column, &MimeDataType data, Rute::DropAction action) -> bool,
    supported_drop_actions() -> Rute::DropActions,
    items(&MimeDataType data) -> <&TableWidgetItemType>,
    index_from_item(&TableWidgetItemType item) -> ModelIndexType,
    item_from_index(&ModelIndexType index) -> &TableWidgetItemType,
    drop_event(&DropEventType event),
    set_model(&AbstractItemModelType model),
}

