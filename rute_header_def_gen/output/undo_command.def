
struct UndoCommand {
    undo(),
    redo(),
    text() -> String,
    action_text() -> String,
    set_text(text: String),
    is_obsolete() -> bool,
    set_obsolete(obsolete: bool),
    id() -> i32,
    merge_with(other: &UndoCommandType) -> bool,
    child_count() -> i32,
    child(index: i32) -> &UndoCommandType,
}

