
struct UndoCommand {
    [event] undo(),
    [event] redo(),
    text() -> String,
    action_text() -> String,
    set_text(text: String),
    is_obsolete() -> bool,
    set_obsolete(obsolete: bool),
    [event] id() -> i32,
    [event] merge_with(other: *UndoCommandType) -> bool,
    child_count() -> i32,
    child(index: i32) -> UndoCommand?,
}

