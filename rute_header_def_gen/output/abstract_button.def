struct AbstractButton : Widget {
    set_text(text: String),
    text() -> String,
    set_icon(icon: &IconType),
    icon() -> IconType,
    icon_size() -> SizeType,
    set_shortcut(key: &KeySequenceType),
    shortcut() -> KeySequenceType,
    set_checkable(arg0: bool),
    is_checkable() -> bool,
    is_checked() -> bool,
    set_down(arg0: bool),
    is_down() -> bool,
    set_auto_repeat(arg0: bool),
    auto_repeat() -> bool,
    set_auto_repeat_delay(arg0: i32),
    auto_repeat_delay() -> i32,
    set_auto_repeat_interval(arg0: i32),
    auto_repeat_interval() -> i32,
    set_auto_exclusive(arg0: bool),
    auto_exclusive() -> bool,
    group() -> &ButtonGroupType,
    set_icon_size(size: &SizeType),
    animate_click(msec: i32),
    click(),
    toggle(),
    set_checked(arg0: bool),
    [callback] pressed(),
    [callback] released(),
    [callback] clicked(checked: bool),
    [callback] toggled(checked: bool),
    paint_event(e: &PaintEventType),
    hit_button(pos: &PointType) -> bool,
    check_state_set(),
    next_check_state(),
    event(e: &EventType) -> bool,
    key_press_event(e: &KeyEventType),
    key_release_event(e: &KeyEventType),
    mouse_press_event(e: &MouseEventType),
    mouse_release_event(e: &MouseEventType),
    mouse_move_event(e: &MouseEventType),
    focus_in_event(e: &FocusEventType),
    focus_out_event(e: &FocusEventType),
    change_event(e: &EventType),
    timer_event(e: &TimerEventType),
}

