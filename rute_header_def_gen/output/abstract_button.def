struct AbstractButton : Widget {
    set_text(String text),
    text() -> String,
    set_icon(&IconType icon),
    icon() -> IconType,
    icon_size() -> SizeType,
    set_shortcut(&KeySequenceType key),
    shortcut() -> KeySequenceType,
    set_checkable(bool arg0),
    is_checkable() -> bool,
    is_checked() -> bool,
    set_down(bool arg0),
    is_down() -> bool,
    set_auto_repeat(bool arg0),
    auto_repeat() -> bool,
    set_auto_repeat_delay(i32 arg0),
    auto_repeat_delay() -> i32,
    set_auto_repeat_interval(i32 arg0),
    auto_repeat_interval() -> i32,
    set_auto_exclusive(bool arg0),
    auto_exclusive() -> bool,
    group() -> &ButtonGroupType,
    set_icon_size(&SizeType size),
    animate_click(i32 msec),
    click(),
    toggle(),
    set_checked(bool arg0),
    pressed(),
    released(),
    clicked(bool checked),
    toggled(bool checked),
    paint_event(&PaintEventType e),
    hit_button(&PointType pos) -> bool,
    check_state_set(),
    next_check_state(),
    event(&EventType e) -> bool,
    key_press_event(&KeyEventType e),
    key_release_event(&KeyEventType e),
    mouse_press_event(&MouseEventType e),
    mouse_release_event(&MouseEventType e),
    mouse_move_event(&MouseEventType e),
    focus_in_event(&FocusEventType e),
    focus_out_event(&FocusEventType e),
    change_event(&EventType e),
    timer_event(&TimerEventType e),
}

