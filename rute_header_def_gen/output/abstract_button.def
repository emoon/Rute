struct AbstractButton : Widget {
    set_text(text: String),
    text() -> String,
    set_icon(icon: &IconType),
    icon() -> IconType,
    icon_size() -> SizeType,
    set_shortcut(key: &KeySequenceType),
    shortcut() -> KeySequenceType,
    set_checkable(arg0: bool),
    is_checkable() -> bool,
    is_checked() -> bool,
    set_down(arg0: bool),
    is_down() -> bool,
    set_auto_repeat(arg0: bool),
    auto_repeat() -> bool,
    set_auto_repeat_delay(arg0: i32),
    auto_repeat_delay() -> i32,
    set_auto_repeat_interval(arg0: i32),
    auto_repeat_interval() -> i32,
    set_auto_exclusive(arg0: bool),
    auto_exclusive() -> bool,
    group() -> &ButtonGroupType,
    set_icon_size(size: &SizeType),
    animate_click(msec: i32),
    click(),
    toggle(),
    set_checked(arg0: bool),
    [event] pressed(),
    [event] released(),
    [event] clicked(checked: bool),
    [event] toggled(checked: bool),
    [override] paint_event(e: &PaintEventType),
    [override] hit_button(pos: &PointType) -> bool,
    [override] check_state_set(),
    [override] next_check_state(),
    [override] event(e: &EventType) -> bool,
    [override] key_press_event(e: &KeyEventType),
    [override] key_release_event(e: &KeyEventType),
    [override] mouse_press_event(e: &MouseEventType),
    [override] mouse_release_event(e: &MouseEventType),
    [override] mouse_move_event(e: &MouseEventType),
    [override] focus_in_event(e: &FocusEventType),
    [override] focus_out_event(e: &FocusEventType),
    [override] change_event(e: &EventType),
    [override] timer_event(e: &TimerEventType),
}

