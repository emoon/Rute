struct Slider : AbstractSlider {
    [override] size_hint() -> SizeType,
    [override] minimum_size_hint() -> SizeType,
    set_tick_position(position: Slider::TickPosition),
    tick_position() -> Slider::TickPosition,
    set_tick_interval(ti: i32),
    tick_interval() -> i32,
    [override] event(event: &EventType) -> bool,
    [override] paint_event(ev: &PaintEventType),
    [override] mouse_press_event(ev: &MouseEventType),
    [override] mouse_release_event(ev: &MouseEventType),
    [override] mouse_move_event(ev: &MouseEventType),
    init_style_option(option: &StyleOptionSliderType),
}

