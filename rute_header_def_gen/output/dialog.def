enum DialogCode {
    Rejected,
    Accepted,
}

struct Dialog : Widget {
    result() -> i32,
    [override] set_visible(visible: bool),
    set_orientation(orientation: Rute::Orientation),
    orientation() -> Rute::Orientation,
    set_extension(extension: &WidgetType),
    extension() -> Widget?,
    [override] size_hint() -> Size,
    [override] minimum_size_hint() -> Size,
    set_size_grip_enabled(arg0: bool),
    is_size_grip_enabled() -> bool,
    set_modal(modal: bool),
    set_result(r: i32),
    [event] finished(result: i32),
    [event] accepted(),
    [event] rejected(),
    [override] open(),
    [override] exec() -> i32,
    [override] done(arg0: i32),
    [override] accept(),
    [override] reject(),
    show_extension(arg0: bool),
    [override] key_press_event(arg0: &KeyEventType),
    [override] close_event(arg0: &CloseEventType),
    [override] show_event(arg0: &ShowEventType),
    [override] resize_event(arg0: &ResizeEventType),
    [override] context_menu_event(arg0: &ContextMenuEventType),
    [override] event_filter(arg0: &ObjectType, arg1: &EventType) -> bool,
    adjust_position(arg0: &WidgetType),
}

