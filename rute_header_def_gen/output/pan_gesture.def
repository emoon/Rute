struct PanGesture : Gesture {
    last_offset() -> PointF,
    offset() -> PointF,
    delta() -> PointF,
    acceleration() -> f32,
    set_last_offset(value: &PointFType),
    set_offset(value: &PointFType),
    set_acceleration(value: f32),
}

