[org_name(QStyleOptionToolButton)]
enum StyleOptionType {
    Type,
}

[org_name(QStyleOptionToolButton)]
enum StyleOptionVersion {
    Version,
}

[org_name(QStyleOptionToolButton)]
enum ToolButtonFeature {
    None,
    Arrow,
    Menu,
    MenuButtonPopup,
    PopupDelay,
    HasMenu,
