enum Notation {
    StandardNotation,
    ScientificNotation,
}

struct DoubleValidator : Validator {
    [replace] validate(arg0: String, arg1: &i32) -> Validator::State,
    [replace] set_range(bottom: double, top: double, decimals: i32),
    set_bottom(arg0: double),
    set_top(arg0: double),
    set_decimals(arg0: i32),
    set_notation(arg0: DoubleValidator::Notation),
    bottom() -> double,
    top() -> double,
    decimals() -> i32,
    notation() -> DoubleValidator::Notation,
    [event] bottom_changed(bottom: double),
    [event] top_changed(top: double),
    [event] decimals_changed(decimals: i32),
    [event] notation_changed(notation: DoubleValidator::Notation),
}

