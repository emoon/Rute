struct GraphicsEffect : Object {
    bounding_rect_for(source_rect: &RectFType) -> RectFType,
    bounding_rect() -> RectFType,
    is_enabled() -> bool,
    set_enabled(enable: bool),
    update(),
    [callback] enabled_changed(enabled: bool),
    draw(painter: &PainterType),
    source_changed(flags: ChangeFlags),
    update_bounding_rect(),
    source_is_pixmap() -> bool,
    source_bounding_rect(system: Rute::CoordinateSystem) -> RectFType,
    draw_source(painter: &PainterType),
    source_pixmap(system: Rute::CoordinateSystem, offset: &PointType, mode: GraphicsEffect::PixmapPadMode) -> PixmapType,
    source() -> &GraphicsEffectSourceType,
}

