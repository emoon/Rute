
struct JsonValueRef {
    to_variant() -> Variant,
    type() -> JsonValue::Type,
    is_null() -> bool,
    is_bool() -> bool,
    is_double() -> bool,
    is_array() -> bool,
    is_object() -> bool,
    is_undefined() -> bool,
    to_bool() -> bool,
    to_int() -> i32,
    to_double() -> double,
    to_array() -> JsonArray,
    to_object() -> JsonObject,
    to_bool(default_value: bool) -> bool,
    to_int(default_value: i32) -> i32,
    to_double(default_value: double) -> double,
}

