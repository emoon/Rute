
struct TextCursor {
    swap(&TextCursorType other),
    is_null() -> bool,
    set_position(i32 pos, TextCursor::MoveMode mode),
    position() -> i32,
    position_in_block() -> i32,
    anchor() -> i32,
    insert_text(String text),
    insert_text(String text, &TextCharFormatType format),
    move_position(TextCursor::MoveOperation op, TextCursor::MoveMode arg0, i32 n) -> bool,
    visual_navigation() -> bool,
    set_visual_navigation(bool b),
    set_vertical_movement_x(i32 x),
    vertical_movement_x() -> i32,
    set_keep_position_on_insert(bool b),
    keep_position_on_insert() -> bool,
    delete_char(),
    delete_previous_char(),
    select(TextCursor::SelectionType selection),
    has_selection() -> bool,
    has_complex_selection() -> bool,
    remove_selected_text(),
    clear_selection(),
    selection_start() -> i32,
    selection_end() -> i32,
    selected_text() -> String,
    selection() -> TextDocumentFragmentType,
    selected_table_cells(&i32 first_row, &i32 num_rows, &i32 first_column, &i32 num_columns),
    block() -> TextBlockType,
    char_format() -> TextCharFormatType,
    set_char_format(&TextCharFormatType format),
    merge_char_format(&TextCharFormatType modifier),
    block_format() -> TextBlockFormatType,
    set_block_format(&TextBlockFormatType format),
    merge_block_format(&TextBlockFormatType modifier),
    block_char_format() -> TextCharFormatType,
    set_block_char_format(&TextCharFormatType format),
    merge_block_char_format(&TextCharFormatType modifier),
    at_block_start() -> bool,
    at_block_end() -> bool,
    at_start() -> bool,
    at_end() -> bool,
    insert_block(),
    insert_block(&TextBlockFormatType format),
    insert_block(&TextBlockFormatType format, &TextCharFormatType char_format),
    insert_list(&TextListFormatType format) -> &TextListType,
    insert_list(TextListFormat::Style style) -> &TextListType,
    create_list(&TextListFormatType format) -> &TextListType,
    create_list(TextListFormat::Style style) -> &TextListType,
    current_list() -> &TextListType,
    insert_table(i32 rows, i32 cols, &TextTableFormatType format) -> &TextTableType,
    insert_table(i32 rows, i32 cols) -> &TextTableType,
    current_table() -> &TextTableType,
    insert_frame(&TextFrameFormatType format) -> &TextFrameType,
    current_frame() -> &TextFrameType,
    insert_fragment(&TextDocumentFragmentType fragment),
    insert_html(String html),
    insert_image(&TextImageFormatType format, TextFrameFormat::Position alignment),
    insert_image(&TextImageFormatType format),
    insert_image(String name),
    insert_image(&ImageType image, String name),
    begin_edit_block(),
    join_previous_edit_block(),
    end_edit_block(),
    is_copy_of(&TextCursorType other) -> bool,
    block_number() -> i32,
    column_number() -> i32,
    document() -> &TextDocumentType,
}

