struct WidgetAction : Action {
    set_default_widget(w: &WidgetType),
    default_widget() -> Widget?,
    request_widget(parent: &WidgetType) -> Widget?,
    release_widget(widget: &WidgetType),
    [replace] event(arg0: &EventType) -> bool,
    [replace] event_filter(arg0: &ObjectType, arg1: &EventType) -> bool,
    [replace] create_widget(parent: &WidgetType) -> Widget?,
    [replace] delete_widget(widget: &WidgetType),
    created_widgets() -> [Widget?],
}

