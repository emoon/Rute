struct WidgetAction : Action {
    set_default_widget(w: &WidgetType),
    default_widget() -> &WidgetType,
    request_widget(parent: &WidgetType) -> &WidgetType,
    release_widget(widget: &WidgetType),
    [override] event(arg0: &EventType) -> bool,
    [override] event_filter(arg0: &ObjectType, arg1: &EventType) -> bool,
    [override] create_widget(parent: &WidgetType) -> &WidgetType,
    [override] delete_widget(widget: &WidgetType),
    created_widgets() -> [&WidgetType],
}

