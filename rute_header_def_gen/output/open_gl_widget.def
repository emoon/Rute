struct OpenGLWidget : Widget {
    set_update_behavior(update_behavior: OpenGLWidget::UpdateBehavior),
    update_behavior() -> OpenGLWidget::UpdateBehavior,
    set_format(format: &SurfaceFormatType),
    format() -> SurfaceFormatType,
    texture_format() -> GLenum,
    set_texture_format(tex_format: GLenum),
    is_valid() -> bool,
    make_current(),
    done_current(),
    context() -> &OpenGLContextType,
    default_framebuffer_object() -> GLuint,
    grab_framebuffer() -> ImageType,
    [callback] about_to_compose(),
    [callback] frame_swapped(),
    [callback] about_to_resize(),
    [callback] resized(),
    initialize_gl(),
    resize_gl(w: i32, h: i32),
    paint_gl(),
    paint_event(e: &PaintEventType),
    resize_event(e: &ResizeEventType),
    event(e: &EventType) -> bool,
    redirected(p: &PointType) -> &PaintDeviceType,
    paint_engine() -> &PaintEngineType,
}

