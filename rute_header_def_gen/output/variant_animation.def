struct VariantAnimation : AbstractAnimation {
    start_value() -> VariantType,
    set_start_value(&VariantType value),
    end_value() -> VariantType,
    set_end_value(&VariantType value),
    key_value_at(f32 step) -> VariantType,
    set_key_value_at(f32 step, &VariantType value),
    key_values() -> KeyValues,
    set_key_values(&KeyValues values),
    current_value() -> VariantType,
    duration() -> i32,
    set_duration(i32 msecs),
    easing_curve() -> EasingCurveType,
    set_easing_curve(&EasingCurveType easing),
    value_changed(&VariantType value),
    event(&EventType event) -> bool,
    update_current_time(i32 arg0),
    update_state(AbstractAnimation::State new_state, AbstractAnimation::State old_state),
    update_current_value(&VariantType value),
    interpolated(&VariantType from, &VariantType to, f32 progress) -> VariantType,
    register_interpolator(Interpolator func, i32 interpolation_type),
}

