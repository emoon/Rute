struct FontDialog : Dialog {
    set_current_font(font: &FontType),
    current_font() -> FontType,
    selected_font() -> FontType,
    set_option(option: FontDialog::FontDialogOption, on: bool),
    test_option(option: FontDialog::FontDialogOption) -> bool,
    set_options(options: FontDialogOptions),
    options() -> FontDialogOptions,
    open(receiver: &ObjectType, member: &char),
    set_visible(visible: bool),
    get_font(ok: &bool, parent: &WidgetType) -> FontType,
    get_font(ok: &bool, initial: &FontType, parent: &WidgetType, title: String, options: FontDialogOptions) -> FontType,
    [callback] current_font_changed(font: &FontType),
    [callback] font_selected(font: &FontType),
    change_event(event: &EventType),
    done(result: i32),
    event_filter(object: &ObjectType, event: &EventType) -> bool,
}

