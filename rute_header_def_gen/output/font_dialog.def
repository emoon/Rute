#[org_name(QFontDialog)]
enum FontDialogOption {
    NoButtons,
    DontUseNativeDialog,
    ScalableFonts,
    NonScalableFonts,
    MonospacedFonts,
    ProportionalFonts,
}

struct FontDialog : Dialog {
    set_current_font(font: &FontType),
    current_font() -> Font,
    selected_font() -> Font,
    set_option(option: FontDialog::FontDialogOption, on: bool),
    test_option(option: FontDialog::FontDialogOption) -> bool,
    set_options(options: FontDialogOptions),
    options() -> FontDialogOptions,
    open(receiver: &ObjectType, member: &char),
    [replace] set_visible(visible: bool),
    [static] get_font(ok: &bool, parent: &WidgetType) -> Font,
    [static] get_font(ok: &bool, initial: &FontType, parent: &WidgetType, title: String, options: FontDialogOptions) -> Font,
    [event] current_font_changed(font: &FontType),
    [event] font_selected(font: &FontType),
    [replace] change_event(event: &EventType),
    [replace] done(result: i32),
    [replace] event_filter(object: &ObjectType, event: &EventType) -> bool,
}

