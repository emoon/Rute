
struct ModelIndex {
    row() -> i32,
    column() -> i32,
    internal_id() -> quintptr,
    internal_pointer(),
    parent() -> ModelIndex,
    sibling(row: i32, column: i32) -> ModelIndex,
    child(row: i32, column: i32) -> ModelIndex,
    data(role: i32) -> Variant,
    flags() -> Rute::ItemFlags,
    model() -> AbstractItemModel?,
    is_valid() -> bool,
}

