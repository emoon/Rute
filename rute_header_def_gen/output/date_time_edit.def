struct DateTimeEdit : AbstractSpinBox {
    date_time() -> DateTimeType,
    date() -> DateType,
    time() -> TimeType,
    minimum_date_time() -> DateTimeType,
    clear_minimum_date_time(),
    set_minimum_date_time(&DateTimeType dt),
    maximum_date_time() -> DateTimeType,
    clear_maximum_date_time(),
    set_maximum_date_time(&DateTimeType dt),
    set_date_time_range(&DateTimeType min, &DateTimeType max),
    minimum_date() -> DateType,
    set_minimum_date(&DateType min),
    clear_minimum_date(),
    maximum_date() -> DateType,
    set_maximum_date(&DateType max),
    clear_maximum_date(),
    set_date_range(&DateType min, &DateType max),
    minimum_time() -> TimeType,
    set_minimum_time(&TimeType min),
    clear_minimum_time(),
    maximum_time() -> TimeType,
    set_maximum_time(&TimeType max),
    clear_maximum_time(),
    set_time_range(&TimeType min, &TimeType max),
    displayed_sections() -> Sections,
    current_section() -> DateTimeEdit::Section,
    section_at(i32 index) -> DateTimeEdit::Section,
    set_current_section(DateTimeEdit::Section section),
    current_section_index() -> i32,
    set_current_section_index(i32 index),
    calendar_widget() -> &CalendarWidgetType,
    set_calendar_widget(&CalendarWidgetType calendar_widget),
    section_count() -> i32,
    set_selected_section(DateTimeEdit::Section section),
    section_text(DateTimeEdit::Section section) -> String,
    display_format() -> String,
    set_display_format(String format),
    calendar_popup() -> bool,
    set_calendar_popup(bool enable),
    time_spec() -> Rute::TimeSpec,
    set_time_spec(Rute::TimeSpec spec),
    size_hint() -> SizeType,
    clear(),
    step_by(i32 steps),
    event(&EventType event) -> bool,
    date_time_changed(&DateTimeType date_time),
    time_changed(&TimeType time),
    date_changed(&DateType date),
    set_date_time(&DateTimeType date_time),
    set_date(&DateType date),
    set_time(&TimeType time),
    key_press_event(&KeyEventType event),
    wheel_event(&WheelEventType event),
    focus_in_event(&FocusEventType event),
    focus_next_prev_child(bool next) -> bool,
    validate(String input, &i32 pos) -> Validator::State,
    fixup(String input),
    date_time_from_text(String text) -> DateTimeType,
    text_from_date_time(&DateTimeType dt) -> String,
    step_enabled() -> StepEnabled,
    mouse_press_event(&MouseEventType event),
    paint_event(&PaintEventType event),
    init_style_option(&StyleOptionSpinBoxType option),
}

