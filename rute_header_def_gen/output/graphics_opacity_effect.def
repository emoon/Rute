struct GraphicsOpacityEffect : GraphicsEffect {
    opacity() -> f32,
    opacity_mask() -> BrushType,
    [callback] set_opacity(f32 opacity),
    [callback] set_opacity_mask(&BrushType mask),
    opacity_changed(f32 opacity),
    opacity_mask_changed(&BrushType mask),
    draw(&PainterType painter),
}

