struct GraphicsOpacityEffect : GraphicsEffect {
    opacity() -> f32,
    opacity_mask() -> BrushType,
    set_opacity(opacity: f32),
    set_opacity_mask(mask: &BrushType),
    [callback] opacity_changed(opacity: f32),
    [callback] opacity_mask_changed(mask: &BrushType),
    draw(painter: &PainterType),
}

