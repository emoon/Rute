
struct MenuSloppyState {
    initialize(&i32 menu),
    reset(),
    enabled() -> bool,
    start_timer(),
    start_timer_if_not_running(),
    stop_timer(),
    enter(),
    child_enter(),
    leave(),
    child_leave(),
    slope(&PointFType p1, &PointFType p2) -> f32,
    check_slope(f32 old_s, f32 new_s, bool want_steeper) -> bool,
    process_mouse_event(&PointFType mouse_pos, &ActionType reset_action, &ActionType current_action) -> MenuSloppyState::MouseEventResult,
    set_sub_menu_popup(&RectType action_rect, &ActionType reset_action, &i32 sub_menu),
    has_parent_active_delay_timer() -> bool,
    timeout(),
    time_for_timeout() -> i32,
    is_timer_id(i32 timer_id) -> bool,
    sub_menu() -> &i32,
}

