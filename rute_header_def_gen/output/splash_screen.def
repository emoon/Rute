struct SplashScreen : Widget {
    set_pixmap(pixmap: &PixmapType),
    pixmap() -> PixmapType,
    finish(w: &WidgetType),
    repaint(),
    message() -> String,
    show_message(message: String, alignment: i32, color: &ColorType),
    clear_message(),
    [callback] message_changed(message: String),
    event(e: &EventType) -> bool,
    draw_contents(painter: &PainterType),
    mouse_press_event(arg0: &MouseEventType),
}

