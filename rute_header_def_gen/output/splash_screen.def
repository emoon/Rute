struct SplashScreen : Widget {
    set_pixmap(pixmap: &PixmapType),
    pixmap() -> PixmapType,
    finish(w: &WidgetType),
    repaint(),
    message() -> String,
    show_message(message: String, alignment: i32, color: &ColorType),
    clear_message(),
    [event] message_changed(message: String),
    [override] event(e: &EventType) -> bool,
    [override] draw_contents(painter: &PainterType),
    [override] mouse_press_event(arg0: &MouseEventType),
}

