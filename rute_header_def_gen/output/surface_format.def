
struct SurfaceFormat {
    qt_check_for_qgadget_macro(),
    set_depth_buffer_size(i32 size),
    depth_buffer_size() -> i32,
    set_stencil_buffer_size(i32 size),
    stencil_buffer_size() -> i32,
    set_red_buffer_size(i32 size),
    red_buffer_size() -> i32,
    set_green_buffer_size(i32 size),
    green_buffer_size() -> i32,
    set_blue_buffer_size(i32 size),
    blue_buffer_size() -> i32,
    set_alpha_buffer_size(i32 size),
    alpha_buffer_size() -> i32,
    set_samples(i32 num_samples),
    samples() -> i32,
    set_swap_behavior(SurfaceFormat::SwapBehavior behavior),
    swap_behavior() -> SurfaceFormat::SwapBehavior,
    has_alpha() -> bool,
    set_profile(SurfaceFormat::OpenGLContextProfile profile),
    profile() -> SurfaceFormat::OpenGLContextProfile,
    set_renderable_type(SurfaceFormat::RenderableType type),
    renderable_type() -> SurfaceFormat::RenderableType,
    set_major_version(i32 major_version),
    major_version() -> i32,
    set_minor_version(i32 minor_version),
    minor_version() -> i32,
    version() -> Pair<int,Type,
    set_version(i32 major, i32 minor),
    stereo() -> bool,
    set_stereo(bool enable),
    set_option(SurfaceFormat::FormatOptions opt),
    test_option(SurfaceFormat::FormatOptions opt) -> bool,
    set_options(SurfaceFormat::FormatOptions options),
    set_option(SurfaceFormat::FormatOption option, bool on),
    test_option(SurfaceFormat::FormatOption option) -> bool,
    options() -> SurfaceFormat::FormatOptions,
    swap_interval() -> i32,
    set_swap_interval(i32 interval),
    color_space() -> SurfaceFormat::ColorSpace,
    set_color_space(SurfaceFormat::ColorSpace color_space),
    set_default_format(&SurfaceFormatType format),
    default_format() -> SurfaceFormatType,
    detach(),
}

