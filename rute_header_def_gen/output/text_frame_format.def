struct TextFrameFormat : TextFormat {
    is_valid() -> bool,
    set_position(TextFrameFormat::Position f),
    position() -> TextFrameFormat::Position,
    set_border(f32 border),
    border() -> f32,
    set_border_brush(&BrushType brush),
    border_brush() -> BrushType,
    set_border_style(TextFrameFormat::BorderStyle style),
    border_style() -> TextFrameFormat::BorderStyle,
    set_margin(f32 margin),
    margin() -> f32,
    set_top_margin(f32 margin),
    top_margin() -> f32,
    set_bottom_margin(f32 margin),
    bottom_margin() -> f32,
    set_left_margin(f32 margin),
    left_margin() -> f32,
    set_right_margin(f32 margin),
    right_margin() -> f32,
    set_padding(f32 padding),
    padding() -> f32,
    set_width(f32 width),
    set_width(&TextLengthType length),
    width() -> TextLengthType,
    set_height(f32 height),
    set_height(&TextLengthType height),
    height() -> TextLengthType,
    set_page_break_policy(PageBreakFlags flags),
    page_break_policy() -> PageBreakFlags,
}

