struct PixmapConvolutionFilter : PixmapFilter {
    set_convolution_kernel(&f32 matrix, i32 rows, i32 columns),
    bounding_rect_for(&RectFType rect) -> RectFType,
    draw(&PainterType painter, &PointFType dest, &PixmapType src, &RectFType src_rect),
    convolution_kernel() -> &f32,
    rows() -> i32,
    columns() -> i32,
}

