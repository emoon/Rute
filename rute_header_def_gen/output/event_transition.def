struct EventTransition : AbstractTransition {
    event_source() -> Object?,
    set_event_source(object: &ObjectType),
    event_type() -> Event::Type,
    set_event_type(type: Event::Type),
    [override] event_test(event: &EventType) -> bool,
    [override] on_transition(event: &EventType),
    [override] event(e: &EventType) -> bool,
}

