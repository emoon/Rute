struct ToolBar : Widget {
    set_movable(bool movable),
    is_movable() -> bool,
    set_allowed_areas(Rute::ToolBarAreas areas),
    allowed_areas() -> Rute::ToolBarAreas,
    is_area_allowed(Rute::ToolBarArea area) -> bool,
    set_orientation(Rute::Orientation orientation),
    orientation() -> Rute::Orientation,
    clear(),
    add_action(String text) -> &ActionType,
    add_action(&IconType icon, String text) -> &ActionType,
    add_action(String text, &ObjectType receiver, &char member) -> &ActionType,
    add_action(&IconType icon, String text, &ObjectType receiver, &char member) -> &ActionType,
    insert_separator(&ActionType before) -> &ActionType,
    add_widget(&WidgetType widget) -> &ActionType,
    insert_widget(&ActionType before, &WidgetType widget) -> &ActionType,
    action_at(&PointType p) -> &ActionType,
    action_at(i32 x, i32 y) -> &ActionType,
    toggle_view_action() -> &ActionType,
    icon_size() -> SizeType,
    tool_button_style() -> Rute::ToolButtonStyle,
    widget_for_action(&ActionType action) -> &WidgetType,
    is_floatable() -> bool,
    set_floatable(bool floatable),
    is_floating() -> bool,
    set_icon_size(&SizeType icon_size),
    set_tool_button_style(Rute::ToolButtonStyle tool_button_style),
    [callback] action_triggered(&ActionType action),
    [callback] movable_changed(bool movable),
    [callback] allowed_areas_changed(Rute::ToolBarAreas allowed_areas),
    [callback] orientation_changed(Rute::Orientation orientation),
    [callback] icon_size_changed(&SizeType icon_size),
    [callback] tool_button_style_changed(Rute::ToolButtonStyle tool_button_style),
    [callback] top_level_changed(bool top_level),
    [callback] visibility_changed(bool visible),
    action_event(&ActionEventType event),
    change_event(&EventType event),
    paint_event(&PaintEventType event),
    event(&EventType event) -> bool,
    init_style_option(&StyleOptionToolBarType option),
}

