struct TreeView : AbstractItemView {
    set_model(&AbstractItemModelType model),
    set_root_index(&ModelIndexType index),
    set_selection_model(&ItemSelectionModelType selection_model),
    header() -> &HeaderViewType,
    set_header(&HeaderViewType header),
    auto_expand_delay() -> i32,
    set_auto_expand_delay(i32 delay),
    indentation() -> i32,
    set_indentation(i32 i),
    reset_indentation(),
    root_is_decorated() -> bool,
    set_root_is_decorated(bool show),
    uniform_row_heights() -> bool,
    set_uniform_row_heights(bool uniform),
    items_expandable() -> bool,
    set_items_expandable(bool enable),
    expands_on_double_click() -> bool,
    set_expands_on_double_click(bool enable),
    column_viewport_position(i32 column) -> i32,
    column_width(i32 column) -> i32,
    set_column_width(i32 column, i32 width),
    column_at(i32 x) -> i32,
    is_column_hidden(i32 column) -> bool,
    set_column_hidden(i32 column, bool hide),
    is_header_hidden() -> bool,
    set_header_hidden(bool hide),
    is_row_hidden(i32 row, &ModelIndexType parent) -> bool,
    set_row_hidden(i32 row, &ModelIndexType parent, bool hide),
    is_first_column_spanned(i32 row, &ModelIndexType parent) -> bool,
    set_first_column_spanned(i32 row, &ModelIndexType parent, bool span),
    is_expanded(&ModelIndexType index) -> bool,
    set_expanded(&ModelIndexType index, bool expand),
    set_sorting_enabled(bool enable),
    is_sorting_enabled() -> bool,
    set_animated(bool enable),
    is_animated() -> bool,
    set_all_columns_show_focus(bool enable),
    all_columns_show_focus() -> bool,
    set_word_wrap(bool on),
    word_wrap() -> bool,
    set_tree_position(i32 logical_index),
    keyboard_search(String search),
    visual_rect(&ModelIndexType index) -> RectType,
    scroll_to(&ModelIndexType index, AbstractItemView::ScrollHint hint),
    index_at(&PointType p) -> ModelIndexType,
    index_above(&ModelIndexType index) -> ModelIndexType,
    index_below(&ModelIndexType index) -> ModelIndexType,
    do_items_layout(),
    reset(),
    sort_by_column(i32 column, Rute::SortOrder order),
    data_changed(&ModelIndexType top_left, &ModelIndexType bottom_right, <i32> roles),
    select_all(),
    [callback] expanded(&ModelIndexType index),
    [callback] collapsed(&ModelIndexType index),
    hide_column(i32 column),
    show_column(i32 column),
    expand(&ModelIndexType index),
    collapse(&ModelIndexType index),
    resize_column_to_contents(i32 column),
    sort_by_column(i32 column),
    expand_all(),
    collapse_all(),
    expand_to_depth(i32 depth),
    column_resized(i32 column, i32 old_size, i32 new_size),
    column_count_changed(i32 old_count, i32 new_count),
    column_moved(),
    reexpand(),
    rows_removed(&ModelIndexType parent, i32 first, i32 last),
    scroll_contents_by(i32 dx, i32 dy),
    rows_inserted(&ModelIndexType parent, i32 start, i32 end),
    rows_about_to_be_removed(&ModelIndexType parent, i32 start, i32 end),
    move_cursor(AbstractItemView::CursorAction cursor_action, Rute::KeyboardModifiers modifiers) -> ModelIndexType,
    horizontal_offset() -> i32,
    vertical_offset() -> i32,
    set_selection(&RectType rect, ItemSelectionModel::SelectionFlags command),
    visual_region_for_selection(&ItemSelectionType selection) -> RegionType,
    selected_indexes() -> ModelIndexListType,
    timer_event(&TimerEventType event),
    paint_event(&PaintEventType event),
    draw_tree(&PainterType painter, &RegionType region),
    draw_row(&PainterType painter, &StyleOptionViewItemType options, &ModelIndexType index),
    draw_branches(&PainterType painter, &RectType rect, &ModelIndexType index),
    mouse_press_event(&MouseEventType event),
    mouse_release_event(&MouseEventType event),
    mouse_double_click_event(&MouseEventType event),
    mouse_move_event(&MouseEventType event),
    key_press_event(&KeyEventType event),
    drag_move_event(&DragMoveEventType event),
    viewport_event(&EventType event) -> bool,
    viewport_size_hint() -> SizeType,
    size_hint_for_column(i32 column) -> i32,
    index_row_size_hint(&ModelIndexType index) -> i32,
    row_height(&ModelIndexType index) -> i32,
    horizontal_scrollbar_action(i32 action),
    is_index_hidden(&ModelIndexType index) -> bool,
    selection_changed(&ItemSelectionType selected, &ItemSelectionType deselected),
    current_changed(&ModelIndexType current, &ModelIndexType previous),
}

