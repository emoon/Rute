struct TreeView : AbstractItemView {
    [override] set_model(model: &AbstractItemModelType),
    [override] set_root_index(index: &ModelIndexType),
    [override] set_selection_model(selection_model: &ItemSelectionModelType),
    header() -> &HeaderViewType,
    set_header(header: &HeaderViewType),
    auto_expand_delay() -> i32,
    set_auto_expand_delay(delay: i32),
    indentation() -> i32,
    set_indentation(i: i32),
    reset_indentation(),
    root_is_decorated() -> bool,
    set_root_is_decorated(show: bool),
    uniform_row_heights() -> bool,
    set_uniform_row_heights(uniform: bool),
    items_expandable() -> bool,
    set_items_expandable(enable: bool),
    expands_on_double_click() -> bool,
    set_expands_on_double_click(enable: bool),
    column_viewport_position(column: i32) -> i32,
    column_width(column: i32) -> i32,
    set_column_width(column: i32, width: i32),
    column_at(x: i32) -> i32,
    is_column_hidden(column: i32) -> bool,
    set_column_hidden(column: i32, hide: bool),
    is_header_hidden() -> bool,
    set_header_hidden(hide: bool),
    is_row_hidden(row: i32, parent: &ModelIndexType) -> bool,
    set_row_hidden(row: i32, parent: &ModelIndexType, hide: bool),
    is_first_column_spanned(row: i32, parent: &ModelIndexType) -> bool,
    set_first_column_spanned(row: i32, parent: &ModelIndexType, span: bool),
    is_expanded(index: &ModelIndexType) -> bool,
    set_expanded(index: &ModelIndexType, expand: bool),
    set_sorting_enabled(enable: bool),
    is_sorting_enabled() -> bool,
    set_animated(enable: bool),
    is_animated() -> bool,
    set_all_columns_show_focus(enable: bool),
    all_columns_show_focus() -> bool,
    set_word_wrap(on: bool),
    word_wrap() -> bool,
    set_tree_position(logical_index: i32),
    [override] keyboard_search(search: String),
    [override] visual_rect(index: &ModelIndexType) -> RectType,
    [override] scroll_to(index: &ModelIndexType, hint: AbstractItemView::ScrollHint),
    [override] index_at(p: &PointType) -> ModelIndexType,
    index_above(index: &ModelIndexType) -> ModelIndexType,
    index_below(index: &ModelIndexType) -> ModelIndexType,
    [override] do_items_layout(),
    [override] reset(),
    sort_by_column(column: i32, order: Rute::SortOrder),
    [override] data_changed(top_left: &ModelIndexType, bottom_right: &ModelIndexType, roles: [i32]),
    [override] select_all(),
    [event] expanded(index: &ModelIndexType),
    [event] collapsed(index: &ModelIndexType),
    hide_column(column: i32),
    show_column(column: i32),
    expand(index: &ModelIndexType),
    collapse(index: &ModelIndexType),
    resize_column_to_contents(column: i32),
    sort_by_column(column: i32),
    expand_all(),
    collapse_all(),
    expand_to_depth(depth: i32),
    column_resized(column: i32, old_size: i32, new_size: i32),
    column_count_changed(old_count: i32, new_count: i32),
    column_moved(),
    reexpand(),
    rows_removed(parent: &ModelIndexType, first: i32, last: i32),
    [override] scroll_contents_by(dx: i32, dy: i32),
    [override] rows_inserted(parent: &ModelIndexType, start: i32, end: i32),
    [override] rows_about_to_be_removed(parent: &ModelIndexType, start: i32, end: i32),
    [override] move_cursor(cursor_action: AbstractItemView::CursorAction, modifiers: Rute::KeyboardModifiers) -> ModelIndexType,
    [override] horizontal_offset() -> i32,
    [override] vertical_offset() -> i32,
    [override] set_selection(rect: &RectType, command: ItemSelectionModel::SelectionFlags),
    [override] visual_region_for_selection(selection: &ItemSelectionType) -> RegionType,
    [override] selected_indexes() -> ModelIndexListType,
    [override] timer_event(event: &TimerEventType),
    [override] paint_event(event: &PaintEventType),
    draw_tree(painter: &PainterType, region: &RegionType),
    [override] draw_row(painter: &PainterType, options: &StyleOptionViewItemType, index: &ModelIndexType),
    [override] draw_branches(painter: &PainterType, rect: &RectType, index: &ModelIndexType),
    [override] mouse_press_event(event: &MouseEventType),
    [override] mouse_release_event(event: &MouseEventType),
    [override] mouse_double_click_event(event: &MouseEventType),
    [override] mouse_move_event(event: &MouseEventType),
    [override] key_press_event(event: &KeyEventType),
    [override] drag_move_event(event: &DragMoveEventType),
    [override] viewport_event(event: &EventType) -> bool,
    [override] viewport_size_hint() -> SizeType,
    [override] size_hint_for_column(column: i32) -> i32,
    index_row_size_hint(index: &ModelIndexType) -> i32,
    row_height(index: &ModelIndexType) -> i32,
    [override] horizontal_scrollbar_action(action: i32),
    [override] is_index_hidden(index: &ModelIndexType) -> bool,
    [override] selection_changed(selected: &ItemSelectionType, deselected: &ItemSelectionType),
    [override] current_changed(current: &ModelIndexType, previous: &ModelIndexType),
}

