    set_shortcut_visible_in_context_menu(show: bool),
    is_shortcut_visible_in_context_menu() -> bool,
    parent_widget() -> &WidgetType,
    associated_widgets() -> [&WidgetType],
    associated_graphics_widgets() -> [&GraphicsWidgetType],
    [override] event(arg0: &EventType) -> bool,
    hover(),
    set_checked(arg0: bool),
    toggle(),
    set_enabled(arg0: bool),
    set_disabled(b: bool),
    set_visible(arg0: bool),
    [event] changed(),
    [event] hovered(),
    [event] toggled(arg0: bool),
