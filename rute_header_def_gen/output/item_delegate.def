struct ItemDelegate : AbstractItemDelegate {
    has_clipping() -> bool,
    set_clipping(clip: bool),
    paint(painter: &PainterType, option: &StyleOptionViewItemType, index: &ModelIndexType),
    size_hint(option: &StyleOptionViewItemType, index: &ModelIndexType) -> SizeType,
    create_editor(parent: &WidgetType, option: &StyleOptionViewItemType, index: &ModelIndexType) -> &WidgetType,
    set_editor_data(editor: &WidgetType, index: &ModelIndexType),
    set_model_data(editor: &WidgetType, model: &AbstractItemModelType, index: &ModelIndexType),
    item_editor_factory() -> &ItemEditorFactoryType,
    set_item_editor_factory(factory: &ItemEditorFactoryType),
    draw_display(painter: &PainterType, option: &StyleOptionViewItemType, rect: &RectType, text: String),
    draw_decoration(painter: &PainterType, option: &StyleOptionViewItemType, rect: &RectType, pixmap: &PixmapType),
    draw_focus(painter: &PainterType, option: &StyleOptionViewItemType, rect: &RectType),
    draw_check(painter: &PainterType, option: &StyleOptionViewItemType, rect: &RectType, state: Rute::CheckState),
    draw_background(painter: &PainterType, option: &StyleOptionViewItemType, index: &ModelIndexType),
    do_layout(option: &StyleOptionViewItemType, check_rect: &RectType, icon_rect: &RectType, text_rect: &RectType, hint: bool),
    rect(option: &StyleOptionViewItemType, index: &ModelIndexType, role: i32) -> RectType,
    event_filter(object: &ObjectType, event: &EventType) -> bool,
    editor_event(event: &EventType, model: &AbstractItemModelType, option: &StyleOptionViewItemType, index: &ModelIndexType) -> bool,
    set_options(index: &ModelIndexType, option: &StyleOptionViewItemType) -> StyleOptionViewItemType,
    decoration(option: &StyleOptionViewItemType, variant: &VariantType) -> PixmapType,
    selected(pixmap: &PixmapType, palette: &PaletteType, enabled: bool) -> &PixmapType,
    do_check(option: &StyleOptionViewItemType, bounding: &RectType, variant: &VariantType) -> RectType,
    text_rectangle(painter: &PainterType, rect: &RectType, font: &FontType, text: String) -> RectType,
}

