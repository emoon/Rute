#[org_name(QContextMenuEvent)]
enum Reason {
    Mouse,
    Keyboard,
    Other,
}

struct ContextMenuEvent : InputEvent {
    x() -> i32,
    y() -> i32,
    global_x() -> i32,
    global_y() -> i32,
    pos() -> Point?,
    global_pos() -> Point?,
    reason() -> ContextMenuEvent::Reason,
}

