
struct MetaEnum {
    name() -> char?,
    is_flag() -> bool,
    is_scoped() -> bool,
    key_count() -> i32,
    key(index: i32) -> char?,
    value(index: i32) -> i32,
    scope() -> char?,
    key_to_value(key: *char, ok: *bool) -> i32,
    value_to_key(value: i32) -> char?,
    keys_to_value(keys: *char, ok: *bool) -> i32,
    value_to_keys(value: i32) -> ByteArray,
    enclosing_meta_object() -> MetaObject?,
    is_valid() -> bool,
}

