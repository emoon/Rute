enum LineWrapMode {
    NoWrap,
    WidgetWidth,
    FixedPixelWidth,
    FixedColumnWidth,
}

enum AutoFormattingFlag {
    AutoNone,
    AutoBulletList,
    AutoAll,
}

struct TextEdit : AbstractScrollArea {
