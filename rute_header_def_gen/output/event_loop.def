struct EventLoop : Object {
    process_events(flags: ProcessEventsFlags) -> bool,
    process_events(flags: ProcessEventsFlags, maximum_time: i32),
    exec(flags: ProcessEventsFlags) -> i32,
    exit(return_code: i32),
    is_running() -> bool,
    wake_up(),
    [override] event(event: &EventType) -> bool,
    quit(),
}

