struct ColorDialog : Dialog {
    set_current_color(color: &ColorType),
    current_color() -> ColorType,
    selected_color() -> ColorType,
    set_option(option: ColorDialog::ColorDialogOption, on: bool),
    test_option(option: ColorDialog::ColorDialogOption) -> bool,
    set_options(options: ColorDialogOptions),
    options() -> ColorDialogOptions,
    open(receiver: &ObjectType, member: &char),
    set_visible(visible: bool),
    get_color(initial: &ColorType, parent: &WidgetType, title: String, options: ColorDialogOptions) -> ColorType,
    get_rgba(rgba: RgbType, ok: &bool, parent: &WidgetType) -> RgbType,
    custom_count() -> i32,
    custom_color(index: i32) -> ColorType,
    set_custom_color(index: i32, color: ColorType),
    standard_color(index: i32) -> ColorType,
    set_standard_color(index: i32, color: ColorType),
    [callback] current_color_changed(color: &ColorType),
    [callback] color_selected(color: &ColorType),
    change_event(event: &EventType),
    done(result: i32),
}

