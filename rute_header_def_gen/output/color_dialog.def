struct ColorDialog : Dialog {
    set_current_color(color: &ColorType),
    current_color() -> ColorType,
    selected_color() -> ColorType,
    set_option(option: ColorDialog::ColorDialogOption, on: bool),
    test_option(option: ColorDialog::ColorDialogOption) -> bool,
    set_options(options: ColorDialogOptions),
    options() -> ColorDialogOptions,
    open(receiver: &ObjectType, member: &char),
    [override] set_visible(visible: bool),
    [static] get_color(initial: &ColorType, parent: &WidgetType, title: String, options: ColorDialogOptions) -> ColorType,
    [static] get_rgba(rgba: RgbType, ok: &bool, parent: &WidgetType) -> RgbType,
    [static] custom_count() -> i32,
    [static] custom_color(index: i32) -> ColorType,
    [static] set_custom_color(index: i32, color: ColorType),
    [static] standard_color(index: i32) -> ColorType,
    [static] set_standard_color(index: i32, color: ColorType),
    [event] current_color_changed(color: &ColorType),
    [event] color_selected(color: &ColorType),
    [override] change_event(event: &EventType),
    [override] done(result: i32),
}

