struct DataWidgetMapper : Object {
    set_model(&AbstractItemModelType model),
    model() -> &AbstractItemModelType,
    set_item_delegate(&AbstractItemDelegateType delegate),
    item_delegate() -> &AbstractItemDelegateType,
    set_root_index(&ModelIndexType index),
    root_index() -> ModelIndexType,
    set_orientation(Rute::Orientation a_orientation),
    orientation() -> Rute::Orientation,
    set_submit_policy(DataWidgetMapper::SubmitPolicy policy),
    submit_policy() -> DataWidgetMapper::SubmitPolicy,
    add_mapping(&WidgetType widget, i32 section),
    add_mapping(&WidgetType widget, i32 section, &ByteArrayType property_name),
    remove_mapping(&WidgetType widget),
    mapped_section(&WidgetType widget) -> i32,
    mapped_property_name(&WidgetType widget) -> ByteArrayType,
    mapped_widget_at(i32 section) -> &WidgetType,
    clear_mapping(),
    current_index() -> i32,
    revert(),
    submit() -> bool,
    to_first(),
    to_last(),
    to_next(),
    to_previous(),
    set_current_index(i32 index),
    set_current_model_index(&ModelIndexType index),
    current_index_changed(i32 index),
}

