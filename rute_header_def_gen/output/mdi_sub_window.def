[org_name(QMdiSubWindow)]
enum SubWindowOption {
    AllowOutsideAreaHorizontally,
    AllowOutsideAreaVertically,
    RubberBandResize,
    RubberBandMove,
}

struct MdiSubWindow : Widget {
    [replace] size_hint() -> Size,
    [replace] minimum_size_hint() -> Size,
    set_widget(widget: *WidgetType),
    widget() -> Widget?,
    maximized_buttons_widget() -> Widget?,
    maximized_system_menu_icon_widget() -> Widget?,
    is_shaded() -> bool,
    set_option(option: MdiSubWindow::SubWindowOption, on: bool),
    test_option(arg0: MdiSubWindow::SubWindowOption) -> bool,
    set_keyboard_single_step(step: i32),
    keyboard_single_step() -> i32,
    set_keyboard_page_step(step: i32),
    keyboard_page_step() -> i32,
    set_system_menu(system_menu: *MenuType),
    system_menu() -> Menu?,
    mdi_area() -> MdiArea?,
    [event] window_state_changed(old_state: Rute::WindowStates, new_state: Rute::WindowStates),
    [event] about_to_activate(),
    show_system_menu(),
    show_shaded(),
    [replace] event_filter(object: *ObjectType, event: *EventType) -> bool,
    [replace] event(event: *EventType) -> bool,
    [replace] show_event(show_event: *ShowEventType),
    [replace] hide_event(hide_event: *HideEventType),
    [replace] change_event(change_event: *EventType),
    [replace] close_event(close_event: *CloseEventType),
    [replace] leave_event(leave_event: *EventType),
    [replace] resize_event(resize_event: *ResizeEventType),
    [replace] timer_event(timer_event: *TimerEventType),
    [replace] move_event(move_event: *MoveEventType),
    [replace] paint_event(paint_event: *PaintEventType),
    [replace] mouse_press_event(mouse_event: *MouseEventType),
    [replace] mouse_double_click_event(mouse_event: *MouseEventType),
    [replace] mouse_release_event(mouse_event: *MouseEventType),
    [replace] mouse_move_event(mouse_event: *MouseEventType),
    [replace] key_press_event(key_event: *KeyEventType),
    [replace] context_menu_event(context_menu_event: *ContextMenuEventType),
    [replace] focus_in_event(focus_in_event: *FocusEventType),
    [replace] focus_out_event(focus_out_event: *FocusEventType),
    [replace] child_event(child_event: *ChildEventType),
}

