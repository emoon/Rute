struct CheckBox : AbstractButton {
    [override] size_hint() -> Size,
    [override] minimum_size_hint() -> Size,
    set_tristate(y: bool),
    is_tristate() -> bool,
    check_state() -> Rute::CheckState,
    set_check_state(state: Rute::CheckState),
    [event] state_changed(arg0: i32),
    [override] event(e: &EventType) -> bool,
    [override] hit_button(pos: &PointType) -> bool,
    [override] check_state_set(),
    [override] next_check_state(),
    [override] paint_event(arg0: &PaintEventType),
    [override] mouse_move_event(arg0: &MouseEventType),
    init_style_option(option: &StyleOptionButtonType),
}

