struct CheckBox : AbstractButton {
    size_hint() -> SizeType,
    minimum_size_hint() -> SizeType,
    set_tristate(y: bool),
    is_tristate() -> bool,
    check_state() -> Rute::CheckState,
    set_check_state(state: Rute::CheckState),
    [callback] state_changed(arg0: i32),
    event(e: &EventType) -> bool,
    hit_button(pos: &PointType) -> bool,
    check_state_set(),
    next_check_state(),
    paint_event(arg0: &PaintEventType),
    mouse_move_event(arg0: &MouseEventType),
    init_style_option(option: &StyleOptionButtonType),
}

