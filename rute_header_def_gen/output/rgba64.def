struct Rgba64 {
    [static] from_rgba64(c: quint64) -> Rgba64,
    [org_name(fromRgba64)]
    [static] from_rgba64_2(red: u16, green: u16, blue: u16, alpha: u16) -> Rgba64,
    [static] from_rgba(red: u8, green: u8, blue: u8, alpha: u8) -> Rgba64,
    [static] from_argb32(rgb: uint) -> Rgba64,
    is_opaque() -> bool,
    is_transparent() -> bool,
    red() -> u16,
    green() -> u16,
    blue() -> u16,
    alpha() -> u16,
    set_red(red: u16),
    set_green(green: u16),
    set_blue(blue: u16),
    set_alpha(alpha: u16),
    red8() -> u8,
    green8() -> u8,
    blue8() -> u8,
    alpha8() -> u8,
    to_argb32() -> uint,
    to_rgb16() -> ushort,
    premultiplied() -> Rgba64,
    unpremultiplied() -> Rgba64,
}

[org_name(QRgba64)]
enum Shifts {
    RedShift,
    GreenShift,
    BlueShift,
    AlphaShift,
}

