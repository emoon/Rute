enum Shifts {
    RedShift,
    GreenShift,
    BlueShift,
    AlphaShift,
}


struct Rgba64 {
    [static] from_rgba64(c: quint64) -> Rgba64Type,
    [static] from_rgba64(red: u16, green: u16, blue: u16, alpha: u16) -> Rgba64Type,
    [static] from_rgba(red: u8, green: u8, blue: u8, alpha: u8) -> Rgba64Type,
    [static] from_argb32(rgb: uint) -> Rgba64Type,
    is_opaque() -> bool,
    is_transparent() -> bool,
    red() -> u16,
    green() -> u16,
    blue() -> u16,
    alpha() -> u16,
    set_red(red: u16),
    set_green(green: u16),
    set_blue(blue: u16),
    set_alpha(alpha: u16),
    red8() -> u8,
    green8() -> u8,
    blue8() -> u8,
    alpha8() -> u8,
    to_argb32() -> uint,
    to_rgb16() -> ushort,
    premultiplied() -> Rgba64Type,
    unpremultiplied() -> Rgba64Type,
}

