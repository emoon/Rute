    rename(old_name: String, new_name: String) -> bool,
    exists(name: String) -> bool,
    drives() -> FileInfoListType,
    list_separator() -> CharType,
    separator() -> CharType,
    set_current(path: String) -> bool,
    current() -> DirType,
    current_path() -> String,
    home() -> DirType,
    home_path() -> String,
    root() -> DirType,
    root_path() -> String,
    temp() -> DirType,
    temp_path() -> String,
    match(filters: &StringListType, file_name: String) -> bool,
    match(filter: String, file_name: String) -> bool,
    clean_path(path: String) -> String,
    refresh(),
