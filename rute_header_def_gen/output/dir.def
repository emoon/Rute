
struct Dir {
    swap(&DirType other),
    set_path(String path),
    path() -> String,
    absolute_path() -> String,
    canonical_path() -> String,
    add_resource_search_path(String path),
    set_search_paths(String prefix, &StringListType search_paths),
    add_search_path(String prefix, String path),
    search_paths(String prefix) -> StringListType,
    dir_name() -> String,
    file_path(String file_name) -> String,
    absolute_file_path(String file_name) -> String,
    relative_file_path(String file_name) -> String,
    to_native_separators(String path_name) -> String,
    from_native_separators(String path_name) -> String,
    cd(String dir_name) -> bool,
    cd_up() -> bool,
    name_filters() -> StringListType,
    set_name_filters(&StringListType name_filters),
    filter() -> Filters,
    set_filter(Filters filter),
    sorting() -> SortFlags,
    set_sorting(SortFlags sort),
    count() -> uint,
    is_empty(Filters filters) -> bool,
    mkdir(String dir_name) -> bool,
    rmdir(String dir_name) -> bool,
    mkpath(String dir_path) -> bool,
    rmpath(String dir_path) -> bool,
    remove_recursively() -> bool,
    is_readable() -> bool,
    exists() -> bool,
    is_root() -> bool,
    is_relative_path(String path) -> bool,
    is_absolute_path(String path) -> bool,
    is_relative() -> bool,
    is_absolute() -> bool,
    make_absolute() -> bool,
    remove(String file_name) -> bool,
    rename(String old_name, String new_name) -> bool,
    exists(String name) -> bool,
    drives() -> FileInfoListType,
    list_separator() -> CharType,
    separator() -> CharType,
    set_current(String path) -> bool,
    current() -> DirType,
    current_path() -> String,
    home() -> DirType,
    home_path() -> String,
    root() -> DirType,
    root_path() -> String,
    temp() -> DirType,
    temp_path() -> String,
    match(&StringListType filters, String file_name) -> bool,
    match(String filter, String file_name) -> bool,
    clean_path(String path) -> String,
    refresh(),
}

