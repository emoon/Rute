    to_native_separators(path_name: String) -> String,
    from_native_separators(path_name: String) -> String,
    cd(dir_name: String) -> bool,
    cd_up() -> bool,
    name_filters() -> StringListType,
    set_name_filters(name_filters: &StringListType),
