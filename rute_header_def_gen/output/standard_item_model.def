struct StandardItemModel : AbstractItemModel {
    set_item_role_names(&Hash<int,Type role_names),
    index(i32 row, i32 column, &ModelIndexType parent) -> ModelIndexType,
    parent(&ModelIndexType child) -> ModelIndexType,
    row_count(&ModelIndexType parent) -> i32,
    column_count(&ModelIndexType parent) -> i32,
    has_children(&ModelIndexType parent) -> bool,
    sibling(i32 row, i32 column, &ModelIndexType idx) -> ModelIndexType,
    data(&ModelIndexType index, i32 role) -> VariantType,
    set_data(&ModelIndexType index, &VariantType value, i32 role) -> bool,
    header_data(i32 section, Rute::Orientation orientation, i32 role) -> VariantType,
    set_header_data(i32 section, Rute::Orientation orientation, &VariantType value, i32 role) -> bool,
    insert_rows(i32 row, i32 count, &ModelIndexType parent) -> bool,
    insert_columns(i32 column, i32 count, &ModelIndexType parent) -> bool,
    remove_rows(i32 row, i32 count, &ModelIndexType parent) -> bool,
    remove_columns(i32 column, i32 count, &ModelIndexType parent) -> bool,
    flags(&ModelIndexType index) -> Rute::ItemFlags,
    supported_drop_actions() -> Rute::DropActions,
    item_data(&ModelIndexType index) -> Map<int,Type,
    set_item_data(&ModelIndexType index, &Map<int,Type roles) -> bool,
    clear(),
    sort(i32 column, Rute::SortOrder order),
    item_from_index(&ModelIndexType index) -> &StandardItemType,
    index_from_item(&StandardItemType item) -> ModelIndexType,
    item(i32 row, i32 column) -> &StandardItemType,
    set_item(i32 row, i32 column, &StandardItemType item),
    set_item(i32 row, &StandardItemType item),
    invisible_root_item() -> &StandardItemType,
    horizontal_header_item(i32 column) -> &StandardItemType,
    set_horizontal_header_item(i32 column, &StandardItemType item),
    vertical_header_item(i32 row) -> &StandardItemType,
    set_vertical_header_item(i32 row, &StandardItemType item),
    set_horizontal_header_labels(&StringListType labels),
    set_vertical_header_labels(&StringListType labels),
    set_row_count(i32 rows),
    set_column_count(i32 columns),
    append_row(&List<QStandardItemType items),
    append_column(&List<QStandardItemType items),
    append_row(&StandardItemType item),
    insert_row(i32 row, &List<QStandardItemType items),
    insert_column(i32 column, &List<QStandardItemType items),
    insert_row(i32 row, &StandardItemType item),
    insert_row(i32 row, &ModelIndexType parent) -> bool,
    insert_column(i32 column, &ModelIndexType parent) -> bool,
    take_item(i32 row, i32 column) -> &StandardItemType,
    take_row(i32 row) -> <&StandardItemType>,
    take_column(i32 column) -> <&StandardItemType>,
    take_horizontal_header_item(i32 column) -> &StandardItemType,
    take_vertical_header_item(i32 row) -> &StandardItemType,
    item_prototype() -> &StandardItemType,
    set_item_prototype(&StandardItemType item),
    find_items(String text, Rute::MatchFlags flags, i32 column) -> <&StandardItemType>,
    sort_role() -> i32,
    set_sort_role(i32 role),
    mime_types() -> StringListType,
    mime_data(&ModelIndexListType indexes) -> &MimeDataType,
    drop_mime_data(&MimeDataType data, Rute::DropAction action, i32 row, i32 column, &ModelIndexType parent) -> bool,
    item_changed(&StandardItemType item),
}

