[org_name(QDebug)]
enum Latin1Content {
    ContainsBinary,
    ContainsLatin1,
}


struct Debug {
    put_ucs4(ucs4: uint),
    put_byte_array(begin: &char, length: size_t, content: Debug::Latin1Content),
    swap(other: &DebugType),
    reset_format() -> Debug?,
    space() -> Debug?,
    nospace() -> Debug?,
    maybe_space() -> Debug?,
    verbosity() -> i32,
    set_verbosity(verbosity_level: i32),
    auto_insert_spaces() -> bool,
    set_auto_insert_spaces(b: bool),
    quote() -> Debug?,
    noquote() -> Debug?,
    maybe_quote(c: char) -> Debug?,
}

