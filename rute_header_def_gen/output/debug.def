
struct Debug {
    put_ucs4(ucs4: uint),
    put_byte_array(begin: &char, length: size_t, content: Debug::Latin1Content),
    swap(other: &DebugType),
    reset_format() -> &DebugType,
    space() -> &DebugType,
    nospace() -> &DebugType,
    maybe_space() -> &DebugType,
    verbosity() -> i32,
    set_verbosity(verbosity_level: i32),
    auto_insert_spaces() -> bool,
    set_auto_insert_spaces(b: bool),
    quote() -> &DebugType,
    noquote() -> &DebugType,
    maybe_quote(c: char) -> &DebugType,
}

