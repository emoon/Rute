[org_name(QStyleOptionButton)]
enum StyleOptionType {
    Type,
}

[org_name(QStyleOptionButton)]
enum StyleOptionVersion {
    Version,
}

[org_name(QStyleOptionButton)]
enum ButtonFeature {
    None,
    Flat,
    HasMenu,
    DefaultButton,
    AutoDefaultButton,
    CommandLinkButton,
}

