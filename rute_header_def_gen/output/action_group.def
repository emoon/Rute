struct ActionGroup : Object {
    add_action(a: &ActionType) -> &ActionType,
    add_action(text: String) -> &ActionType,
    add_action(icon: &IconType, text: String) -> &ActionType,
    remove_action(a: &ActionType),
    actions() -> [&ActionType],
    checked_action() -> &ActionType,
    is_exclusive() -> bool,
    is_enabled() -> bool,
    is_visible() -> bool,
    set_enabled(arg0: bool),
    set_disabled(b: bool),
    set_visible(arg0: bool),
    set_exclusive(arg0: bool),
    [callback] hovered(arg0: &ActionType),
}

