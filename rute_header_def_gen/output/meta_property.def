
struct MetaProperty {
    name() -> &char,
    type_name() -> &char,
    type() -> Variant::Type,
    user_type() -> i32,
    property_index() -> i32,
    is_readable() -> bool,
    is_writable() -> bool,
    is_resettable() -> bool,
    is_designable(obj: &ObjectType) -> bool,
    is_scriptable(obj: &ObjectType) -> bool,
    is_stored(obj: &ObjectType) -> bool,
    is_editable(obj: &ObjectType) -> bool,
    is_user(obj: &ObjectType) -> bool,
    is_constant() -> bool,
    is_final() -> bool,
    is_flag_type() -> bool,
    is_enum_type() -> bool,
    enumerator() -> MetaEnumType,
    has_notify_signal() -> bool,
    notify_signal() -> MetaMethodType,
    notify_signal_index() -> i32,
    revision() -> i32,
    read(obj: &ObjectType) -> VariantType,
    write(obj: &ObjectType, value: &VariantType) -> bool,
    reset(obj: &ObjectType) -> bool,
    read_on_gadget(gadget: &void) -> VariantType,
    write_on_gadget(gadget: &void, value: &VariantType) -> bool,
    reset_on_gadget(gadget: &void) -> bool,
    has_std_cpp_set() -> bool,
    is_valid() -> bool,
    enclosing_meta_object() -> &MetaObjectType,
}

