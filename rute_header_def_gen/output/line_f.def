
struct LineF {
    from_polar(f32 length, f32 angle) -> LineFType,
    is_null() -> bool,
    p1() -> PointFType,
    p2() -> PointFType,
    x1() -> f32,
    y1() -> f32,
    x2() -> f32,
    y2() -> f32,
    dx() -> f32,
    dy() -> f32,
    length() -> f32,
    set_length(f32 len),
    angle() -> f32,
    set_angle(f32 angle),
    angle_to(&LineFType l) -> f32,
    unit_vector() -> LineFType,
    normal_vector() -> LineFType,
    intersect(&LineFType l, &PointFType intersection_point) -> LineF::IntersectType,
    angle(&LineFType l) -> f32,
    point_at(f32 t) -> PointFType,
    center() -> PointFType,
    set_p1(&PointFType p1),
    set_p2(&PointFType p2),
    set_points(&PointFType p1, &PointFType p2),
    set_line(f32 x1, f32 y1, f32 x2, f32 y2),
    to_line() -> LineType,
}

