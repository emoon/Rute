struct LCDNumber : Frame {
    small_decimal_point() -> bool,
    digit_count() -> i32,
    set_digit_count(i32 n_digits),
    check_overflow(double num) -> bool,
    check_overflow(i32 num) -> bool,
    mode() -> LCDNumber::Mode,
    set_mode(LCDNumber::Mode arg0),
    segment_style() -> LCDNumber::SegmentStyle,
    set_segment_style(LCDNumber::SegmentStyle arg0),
    value() -> double,
    int_value() -> i32,
    size_hint() -> SizeType,
    [callback] display(String str),
    [callback] display(i32 num),
    [callback] display(double num),
    [callback] set_hex_mode(),
    [callback] set_dec_mode(),
    [callback] set_oct_mode(),
    [callback] set_bin_mode(),
    [callback] set_small_decimal_point(bool arg0),
    overflow(),
    event(&EventType e) -> bool,
    paint_event(&PaintEventType arg0),
}

