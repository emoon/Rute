struct ToolBarLayout : Layout {
    add_item(&LayoutItemType item),
    item_at(i32 index) -> &LayoutItemType,
    take_at(i32 index) -> &LayoutItemType,
    count() -> i32,
    is_empty() -> bool,
    invalidate(),
    expanding_directions() -> Rute::Orientations,
    minimum_size() -> SizeType,
    size_hint() -> SizeType,
    insert_action(i32 index, &ActionType action),
    index_of(&ActionType action) -> i32,
    index_of(&WidgetType widget) -> i32,
    layout_actions(&SizeType size) -> bool,
    expanded_size(&SizeType size) -> SizeType,
    set_use_popup_menu(bool set),
    check_use_popup_menu(),
    movable() -> bool,
    update_margin_and_spacing(),
    has_expand_flag() -> bool,
    set_expanded(bool b),
    update_geom_array(),
    create_item(&ActionType action) -> &ToolBarItemType,
}

