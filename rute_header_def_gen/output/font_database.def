
struct FontDatabase {
    qt_check_for_qgadget_macro(),
    standard_sizes() -> [i32],
    writing_systems() -> List<QFontDatabase::WritingSystem>,
    writing_systems(family: String) -> List<QFontDatabase::WritingSystem>,
    families(writing_system: FontDatabase::WritingSystem) -> StringListType,
    styles(family: String) -> StringListType,
    point_sizes(family: String, style: String) -> [i32],
    smooth_sizes(family: String, style: String) -> [i32],
    font(family: String, style: String, point_size: i32) -> FontType,
    is_bitmap_scalable(family: String, style: String) -> bool,
    is_smoothly_scalable(family: String, style: String) -> bool,
    is_scalable(family: String, style: String) -> bool,
    is_fixed_pitch(family: String, style: String) -> bool,
    italic(family: String, style: String) -> bool,
    bold(family: String, style: String) -> bool,
    weight(family: String, style: String) -> i32,
    has_family(family: String) -> bool,
    writing_system_name(writing_system: FontDatabase::WritingSystem) -> String,
    writing_system_sample(writing_system: FontDatabase::WritingSystem) -> String,
    add_application_font(file_name: String) -> i32,
    add_application_font_from_data(font_data: &ByteArrayType) -> i32,
    application_font_families(id: i32) -> StringListType,
    remove_application_font(id: i32) -> bool,
    remove_all_application_fonts() -> bool,
    supports_threaded_font_rendering() -> bool,
    system_font(type: FontDatabase::SystemFont) -> FontType,
}

