struct GraphicsWidget : GraphicsObject, GraphicsLayoutItem {
    layout() -> &GraphicsLayoutType,
    set_layout(layout: &GraphicsLayoutType),
    adjust_size(),
    layout_direction() -> Rute::LayoutDirection,
    set_layout_direction(direction: Rute::LayoutDirection),
    unset_layout_direction(),
    style() -> &StyleType,
    set_style(style: &StyleType),
    font() -> FontType,
    set_font(font: &FontType),
    palette() -> PaletteType,
    set_palette(palette: &PaletteType),
    auto_fill_background() -> bool,
    set_auto_fill_background(enabled: bool),
    resize(size: &SizeFType),
    resize(w: f32, h: f32),
    size() -> SizeFType,
    rect() -> RectFType,
    set_contents_margins(left: f32, top: f32, right: f32, bottom: f32),
    get_contents_margins(left: &f32, top: &f32, right: &f32, bottom: &f32),
    set_window_frame_margins(left: f32, top: f32, right: f32, bottom: f32),
    get_window_frame_margins(left: &f32, top: &f32, right: &f32, bottom: &f32),
    unset_window_frame_margins(),
    window_frame_rect() -> RectFType,
    window_flags() -> Rute::WindowFlags,
    window_type() -> Rute::WindowType,
    set_window_flags(w_flags: Rute::WindowFlags),
    is_active_window() -> bool,
    set_window_title(title: String),
    window_title() -> String,
    focus_policy() -> Rute::FocusPolicy,
    set_focus_policy(policy: Rute::FocusPolicy),
    set_tab_order(first: &GraphicsWidgetType, second: &GraphicsWidgetType),
    focus_widget() -> &GraphicsWidgetType,
    grab_shortcut(sequence: &KeySequenceType, context: Rute::ShortcutContext) -> i32,
    release_shortcut(id: i32),
    set_shortcut_enabled(id: i32, enabled: bool),
    set_shortcut_auto_repeat(id: i32, enabled: bool),
    add_action(action: &ActionType),
    add_actions(actions: [&ActionType]),
    insert_actions(before: &ActionType, actions: [&ActionType]),
    insert_action(before: &ActionType, action: &ActionType),
    remove_action(action: &ActionType),
    actions() -> [&ActionType],
    type() -> i32,
    paint(painter: &PainterType, option: &StyleOptionGraphicsItemType, widget: &WidgetType),
    paint_window_frame(painter: &PainterType, option: &StyleOptionGraphicsItemType, widget: &WidgetType),
    bounding_rect() -> RectFType,
    shape() -> PainterPathType,
    [callback] layout_changed(),
    close() -> bool,
    init_style_option(option: &StyleOptionType),
    size_hint(which: Rute::SizeHint, constraint: &SizeFType) -> SizeFType,
    item_change(change: GraphicsItem::GraphicsItemChange, value: &VariantType) -> VariantType,
    property_change(property_name: String, value: &VariantType) -> VariantType,
    scene_event(event: &EventType) -> bool,
    window_frame_event(e: &EventType) -> bool,
    window_frame_section_at(pos: &PointFType) -> Rute::WindowFrameSection,
    event(event: &EventType) -> bool,
    change_event(event: &EventType),
    close_event(event: &CloseEventType),
    focus_in_event(event: &FocusEventType),
    focus_next_prev_child(next: bool) -> bool,
    focus_out_event(event: &FocusEventType),
    hide_event(event: &HideEventType),
    move_event(event: &GraphicsSceneMoveEventType),
    polish_event(),
    resize_event(event: &GraphicsSceneResizeEventType),
    show_event(event: &ShowEventType),
    hover_move_event(event: &GraphicsSceneHoverEventType),
    hover_leave_event(event: &GraphicsSceneHoverEventType),
    grab_mouse_event(event: &EventType),
    ungrab_mouse_event(event: &EventType),
    grab_keyboard_event(event: &EventType),
    ungrab_keyboard_event(event: &EventType),
}

