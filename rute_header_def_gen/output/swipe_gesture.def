enum SwipeDirection {
    NoDirection,
    Left,
    Right,
    Up,
    Down,
}

struct SwipeGesture : Gesture {
    horizontal_direction() -> SwipeGesture::SwipeDirection,
    vertical_direction() -> SwipeGesture::SwipeDirection,
    swipe_angle() -> f32,
    set_swipe_angle(value: f32),
}

