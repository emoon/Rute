
struct Simplex {
    solve_min() -> f32,
    solve_max() -> f32,
    set_objective(&SimplexConstraintType objective),
    value_at(i32 row, i32 column) -> f32,
    set_value_at(i32 row, i32 column, f32 value),
    clear_row(i32 row_index),
    clear_columns(i32 first, i32 last),
    combine_rows(i32 to_index, i32 from_index, f32 factor),
    find_pivot_column() -> i32,
    pivot_row_for_column(i32 column) -> i32,
    reduced_row_echelon(),
    iterate() -> bool,
    solve_max_helper(),
    solver(Simplex::SolverFactor factor) -> f32,
    collect_results(),
}

