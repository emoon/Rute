struct PushButton : AbstractButton {
    size_hint() -> SizeType,
    minimum_size_hint() -> SizeType,
    auto_default() -> bool,
    set_auto_default(arg0: bool),
    is_default() -> bool,
    set_default(arg0: bool),
    set_menu(menu: &MenuType),
    menu() -> &MenuType,
    set_flat(arg0: bool),
    is_flat() -> bool,
    show_menu(),
    event(e: &EventType) -> bool,
    paint_event(arg0: &PaintEventType),
    key_press_event(arg0: &KeyEventType),
    focus_in_event(arg0: &FocusEventType),
    focus_out_event(arg0: &FocusEventType),
    init_style_option(option: &StyleOptionButtonType),
}

