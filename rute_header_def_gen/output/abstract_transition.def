struct AbstractTransition : Object {
    source_state() -> &StateType,
    target_state() -> &AbstractStateType,
    set_target_state(target: &AbstractStateType),
    target_states() -> [&AbstractStateType],
    set_target_states(targets: [&AbstractStateType]),
    set_transition_type(type: AbstractTransition::TransitionType),
    machine() -> &StateMachineType,
    add_animation(animation: &AbstractAnimationType),
    remove_animation(animation: &AbstractAnimationType),
    animations() -> [&AbstractAnimationType],
    [callback] target_state_changed(arg0: AbstractTransition::QPrivateSignal),
    [callback] target_states_changed(arg0: AbstractTransition::QPrivateSignal),
    event_test(event: &EventType) -> bool,
    on_transition(event: &EventType),
    event(e: &EventType) -> bool,
}

