    [override] close_editor(editor: &WidgetType, hint: AbstractItemDelegate::EndEditHint),
    [override] commit_data(editor: &WidgetType),
    [event] pressed(index: &ModelIndexType),
    [event] clicked(index: &ModelIndexType),
    [event] double_clicked(index: &ModelIndexType),
    [event] activated(index: &ModelIndexType),
    [event] entered(index: &ModelIndexType),
    [event] viewport_entered(),
    [event] icon_size_changed(size: &SizeType),
    set_horizontal_steps_per_item(steps: i32),
    horizontal_steps_per_item() -> i32,
    set_vertical_steps_per_item(steps: i32),
    vertical_steps_per_item() -> i32,
    [override] move_cursor(cursor_action: AbstractItemView::CursorAction, modifiers: Rute::KeyboardModifiers) -> ModelIndexType,
    [override] horizontal_offset() -> i32,
    [override] vertical_offset() -> i32,
    [override] is_index_hidden(index: &ModelIndexType) -> bool,
    [override] set_selection(rect: &RectType, command: ItemSelectionModel::SelectionFlags),
    [override] visual_region_for_selection(selection: &ItemSelectionType) -> RegionType,
    [override] selected_indexes() -> ModelIndexListType,
    [override] edit(index: &ModelIndexType, trigger: AbstractItemView::EditTrigger, event: &EventType) -> bool,
    [override] selection_command(index: &ModelIndexType, event: &EventType) -> ItemSelectionModel::SelectionFlags,
    [override] start_drag(supported_actions: Rute::DropActions),
    [override] view_options() -> StyleOptionViewItemType,
    state() -> AbstractItemView::State,
    set_state(state: AbstractItemView::State),
    schedule_delayed_items_layout(),
    execute_delayed_items_layout(),
    set_dirty_region(region: &RegionType),
    scroll_dirty_region(dx: i32, dy: i32),
    dirty_region_offset() -> PointType,
    start_auto_scroll(),
    stop_auto_scroll(),
    do_auto_scroll(),
    [override] focus_next_prev_child(next: bool) -> bool,
    [override] event(event: &EventType) -> bool,
    [override] viewport_event(event: &EventType) -> bool,
    [override] mouse_press_event(event: &MouseEventType),
    [override] mouse_move_event(event: &MouseEventType),
    [override] mouse_release_event(event: &MouseEventType),
    [override] mouse_double_click_event(event: &MouseEventType),
    [override] drag_enter_event(event: &DragEnterEventType),
    [override] drag_move_event(event: &DragMoveEventType),
    [override] drag_leave_event(event: &DragLeaveEventType),
    [override] drop_event(event: &DropEventType),
    [override] focus_in_event(event: &FocusEventType),
    [override] focus_out_event(event: &FocusEventType),
    [override] key_press_event(event: &KeyEventType),
    [override] resize_event(event: &ResizeEventType),
    [override] timer_event(event: &TimerEventType),
    [override] input_method_event(event: &InputMethodEventType),
