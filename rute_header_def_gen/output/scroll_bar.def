struct ScrollBar : AbstractSlider {
    size_hint() -> SizeType,
    event(event: &EventType) -> bool,
    wheel_event(arg0: &WheelEventType),
    paint_event(arg0: &PaintEventType),
    mouse_press_event(arg0: &MouseEventType),
    mouse_release_event(arg0: &MouseEventType),
    mouse_move_event(arg0: &MouseEventType),
    hide_event(arg0: &HideEventType),
    slider_change(change: AbstractSlider::SliderChange),
    context_menu_event(arg0: &ContextMenuEventType),
    init_style_option(option: &StyleOptionSliderType),
}

