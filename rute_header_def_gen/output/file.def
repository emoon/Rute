struct File : FileDevice {
    file_name() -> String,
    set_file_name(String name),
    encode_name(String file_name) -> ByteArrayType,
    decode_name(&ByteArrayType local_file_name) -> String,
    decode_name(&char local_file_name) -> String,
    exists() -> bool,
    exists(String file_name) -> bool,
    read_link() -> String,
    read_link(String file_name) -> String,
    sym_link_target() -> String,
    sym_link_target(String file_name) -> String,
    remove() -> bool,
    remove(String file_name) -> bool,
    rename(String new_name) -> bool,
    rename(String old_name, String new_name) -> bool,
    link(String new_name) -> bool,
    link(String oldname, String new_name) -> bool,
    copy(String new_name) -> bool,
    copy(String file_name, String new_name) -> bool,
    open(OpenMode flags) -> bool,
    open(&FILE f, OpenMode io_flags, FileHandleFlags handle_flags) -> bool,
    open(i32 fd, OpenMode io_flags, FileHandleFlags handle_flags) -> bool,
    size() -> qint64,
    resize(qint64 sz) -> bool,
    resize(String filename, qint64 sz) -> bool,
    permissions() -> Permissions,
    permissions(String filename) -> Permissions,
    set_permissions(Permissions permission_spec) -> bool,
    set_permissions(String filename, Permissions permission_spec) -> bool,
}

