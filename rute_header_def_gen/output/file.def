struct File : FileDevice {
    file_name() -> String,
    set_file_name(name: String),
    encode_name(file_name: String) -> ByteArrayType,
    decode_name(local_file_name: &ByteArrayType) -> String,
    decode_name(local_file_name: &char) -> String,
    exists() -> bool,
    exists(file_name: String) -> bool,
    read_link() -> String,
    read_link(file_name: String) -> String,
    sym_link_target() -> String,
    sym_link_target(file_name: String) -> String,
    remove() -> bool,
    remove(file_name: String) -> bool,
    rename(new_name: String) -> bool,
    rename(old_name: String, new_name: String) -> bool,
    link(new_name: String) -> bool,
    link(oldname: String, new_name: String) -> bool,
    copy(new_name: String) -> bool,
    copy(file_name: String, new_name: String) -> bool,
    open(flags: OpenMode) -> bool,
    open(f: &FILE, io_flags: OpenMode, handle_flags: FileHandleFlags) -> bool,
    open(fd: i32, io_flags: OpenMode, handle_flags: FileHandleFlags) -> bool,
    size() -> i64,
    resize(sz: i64) -> bool,
    resize(filename: String, sz: i64) -> bool,
    permissions() -> Permissions,
    permissions(filename: String) -> Permissions,
    set_permissions(permission_spec: Permissions) -> bool,
    set_permissions(filename: String, permission_spec: Permissions) -> bool,
}

