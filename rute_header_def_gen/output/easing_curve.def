
struct EasingCurve {
    qt_check_for_qgadget_macro(),
    swap(&EasingCurveType other),
    amplitude() -> f32,
    set_amplitude(f32 amplitude),
    period() -> f32,
    set_period(f32 period),
    overshoot() -> f32,
    set_overshoot(f32 overshoot),
    add_cubic_bezier_segment(&PointFType c1, &PointFType c2, &PointFType end_point),
    add_tcb_segment(&PointFType next_point, f32 t, f32 c, f32 b),
    to_cubic_spline() -> Vector<QPointF>Type,
    type() -> EasingCurve::Type,
    set_type(EasingCurve::Type type),
    set_custom_type(EasingFunction func),
    custom_type() -> EasingFunction,
    value_for_progress(f32 progress) -> f32,
}

