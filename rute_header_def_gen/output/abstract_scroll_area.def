enum SizeAdjustPolicy {
    AdjustIgnored,
    AdjustToContentsOnFirstShow,
    AdjustToContents,
}

struct AbstractScrollArea : Frame {
    vertical_scroll_bar_policy() -> Rute::ScrollBarPolicy,
    set_vertical_scroll_bar_policy(arg0: Rute::ScrollBarPolicy),
    vertical_scroll_bar() -> ScrollBar?,
    set_vertical_scroll_bar(scrollbar: &ScrollBarType),
    horizontal_scroll_bar_policy() -> Rute::ScrollBarPolicy,
    set_horizontal_scroll_bar_policy(arg0: Rute::ScrollBarPolicy),
    horizontal_scroll_bar() -> ScrollBar?,
    set_horizontal_scroll_bar(scrollbar: &ScrollBarType),
    corner_widget() -> Widget?,
    set_corner_widget(widget: &WidgetType),
    add_scroll_bar_widget(widget: &WidgetType, alignment: Rute::Alignment),
    scroll_bar_widgets(alignment: Rute::Alignment) -> WidgetList,
    viewport() -> Widget?,
    set_viewport(widget: &WidgetType),
    maximum_viewport_size() -> Size,
    [override] minimum_size_hint() -> Size,
    [override] size_hint() -> Size,
    [override] setup_viewport(viewport: &WidgetType),
    size_adjust_policy() -> AbstractScrollArea::SizeAdjustPolicy,
    set_size_adjust_policy(policy: AbstractScrollArea::SizeAdjustPolicy),
    set_viewport_margins(left: i32, top: i32, right: i32, bottom: i32),
    set_viewport_margins(margins: &MarginsType),
    viewport_margins() -> Margins,
    [override] event_filter(arg0: &ObjectType, arg1: &EventType) -> bool,
    [override] event(arg0: &EventType) -> bool,
    [override] viewport_event(arg0: &EventType) -> bool,
    [override] resize_event(arg0: &ResizeEventType),
    [override] paint_event(arg0: &PaintEventType),
    [override] mouse_press_event(arg0: &MouseEventType),
    [override] mouse_release_event(arg0: &MouseEventType),
    [override] mouse_double_click_event(arg0: &MouseEventType),
    [override] mouse_move_event(arg0: &MouseEventType),
    [override] wheel_event(arg0: &WheelEventType),
    [override] context_menu_event(arg0: &ContextMenuEventType),
    [override] drag_enter_event(arg0: &DragEnterEventType),
    [override] drag_move_event(arg0: &DragMoveEventType),
    [override] drag_leave_event(arg0: &DragLeaveEventType),
    [override] drop_event(arg0: &DropEventType),
    [override] key_press_event(arg0: &KeyEventType),
    [override] scroll_contents_by(dx: i32, dy: i32),
    [override] viewport_size_hint() -> Size,
}

