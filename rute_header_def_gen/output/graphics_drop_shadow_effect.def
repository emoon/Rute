struct GraphicsDropShadowEffect : GraphicsEffect {
    bounding_rect_for(&RectFType rect) -> RectFType,
    offset() -> PointFType,
    x_offset() -> f32,
    y_offset() -> f32,
    blur_radius() -> f32,
    color() -> ColorType,
    set_offset(&PointFType ofs),
    set_offset(f32 dx, f32 dy),
    set_offset(f32 d),
    set_x_offset(f32 dx),
    set_y_offset(f32 dy),
    set_blur_radius(f32 blur_radius),
    set_color(&ColorType color),
    [callback] offset_changed(&PointFType offset),
    [callback] blur_radius_changed(f32 blur_radius),
    [callback] color_changed(&ColorType color),
    draw(&PainterType painter),
}

