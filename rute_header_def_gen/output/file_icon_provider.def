enum IconType {
    Computer,
    Desktop,
    Trashcan,
    Network,
    Drive,
    Folder,
    File,
}

enum Option {
    DontUseCustomDirectoryIcons,
}


struct FileIconProvider {
    [replace] icon(type: FileIconProvider::IconType) -> Icon,
    [replace] icon(info: &FileInfoType) -> Icon,
    [replace] type(info: &FileInfoType) -> String,
    set_options(options: Options),
    options() -> Options,
}

