
struct TextStream {
    set_codec(&TextCodecType codec),
    set_codec(&char codec_name),
    codec() -> &TextCodecType,
    set_auto_detect_unicode(bool enabled),
    auto_detect_unicode() -> bool,
    set_generate_byte_order_mark(bool generate),
    generate_byte_order_mark() -> bool,
    set_locale(&LocaleType locale),
    locale() -> LocaleType,
    set_device(&IODeviceType device),
    device() -> &IODeviceType,
    status() -> TextStream::Status,
    set_status(TextStream::Status status),
    reset_status(),
    at_end() -> bool,
    reset(),
    flush(),
    seek(qint64 pos) -> bool,
    pos() -> qint64,
    skip_white_space(),
    read_line(qint64 maxlen) -> String,
    read_line_into(String line, qint64 maxlen) -> bool,
    read_all() -> String,
    read(qint64 maxlen) -> String,
    set_field_alignment(TextStream::FieldAlignment alignment),
    field_alignment() -> TextStream::FieldAlignment,
    set_pad_char(CharType ch),
    pad_char() -> CharType,
    set_field_width(i32 width),
    field_width() -> i32,
    set_number_flags(NumberFlags flags),
    number_flags() -> NumberFlags,
    set_integer_base(i32 base),
    integer_base() -> i32,
    set_real_number_notation(TextStream::RealNumberNotation notation),
    real_number_notation() -> TextStream::RealNumberNotation,
    set_real_number_precision(i32 precision),
    real_number_precision() -> i32,
}

