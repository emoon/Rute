struct DockWidget : Widget {
    widget() -> &WidgetType,
    set_widget(&WidgetType widget),
    set_features(DockWidgetFeatures features),
    features() -> DockWidgetFeatures,
    set_floating(bool floating),
    is_floating() -> bool,
    set_allowed_areas(Rute::DockWidgetAreas areas),
    allowed_areas() -> Rute::DockWidgetAreas,
    set_title_bar_widget(&WidgetType widget),
    title_bar_widget() -> &WidgetType,
    is_area_allowed(Rute::DockWidgetArea area) -> bool,
    toggle_view_action() -> &ActionType,
    features_changed(DockWidget::DockWidgetFeatures features),
    top_level_changed(bool top_level),
    allowed_areas_changed(Rute::DockWidgetAreas allowed_areas),
    visibility_changed(bool visible),
    dock_location_changed(Rute::DockWidgetArea area),
    change_event(&EventType event),
    close_event(&CloseEventType event),
    paint_event(&PaintEventType event),
    event(&EventType event) -> bool,
    init_style_option(&StyleOptionDockWidgetType option),
}

