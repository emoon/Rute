struct ListView : AbstractItemView {
    set_movement(movement: ListView::Movement),
    movement() -> ListView::Movement,
    set_flow(flow: ListView::Flow),
    flow() -> ListView::Flow,
    set_wrapping(enable: bool),
    is_wrapping() -> bool,
    set_resize_mode(mode: ListView::ResizeMode),
    resize_mode() -> ListView::ResizeMode,
    set_layout_mode(mode: ListView::LayoutMode),
    layout_mode() -> ListView::LayoutMode,
    set_spacing(space: i32),
    spacing() -> i32,
    set_batch_size(batch_size: i32),
    batch_size() -> i32,
    set_grid_size(size: &SizeType),
    grid_size() -> SizeType,
    set_view_mode(mode: ListView::ViewMode),
    view_mode() -> ListView::ViewMode,
    clear_property_flags(),
    is_row_hidden(row: i32) -> bool,
    set_row_hidden(row: i32, hide: bool),
    set_model_column(column: i32),
    model_column() -> i32,
    set_uniform_item_sizes(enable: bool),
    uniform_item_sizes() -> bool,
    set_word_wrap(on: bool),
    word_wrap() -> bool,
    set_selection_rect_visible(show: bool),
    is_selection_rect_visible() -> bool,
    visual_rect(index: &ModelIndexType) -> RectType,
    scroll_to(index: &ModelIndexType, hint: AbstractItemView::ScrollHint),
    index_at(p: &PointType) -> ModelIndexType,
    do_items_layout(),
    reset(),
    set_root_index(index: &ModelIndexType),
    [callback] indexes_moved(indexes: &ModelIndexListType),
    event(e: &EventType) -> bool,
    scroll_contents_by(dx: i32, dy: i32),
    resize_contents(width: i32, height: i32),
    contents_size() -> SizeType,
    data_changed(top_left: &ModelIndexType, bottom_right: &ModelIndexType, roles: [i32]),
    rows_inserted(parent: &ModelIndexType, start: i32, end: i32),
    rows_about_to_be_removed(parent: &ModelIndexType, start: i32, end: i32),
    mouse_move_event(e: &MouseEventType),
    mouse_release_event(e: &MouseEventType),
    wheel_event(e: &WheelEventType),
    timer_event(e: &TimerEventType),
    resize_event(e: &ResizeEventType),
    drag_move_event(e: &DragMoveEventType),
    drag_leave_event(e: &DragLeaveEventType),
    drop_event(e: &DropEventType),
    start_drag(supported_actions: Rute::DropActions),
    view_options() -> StyleOptionViewItemType,
    paint_event(e: &PaintEventType),
    horizontal_offset() -> i32,
    vertical_offset() -> i32,
    move_cursor(cursor_action: AbstractItemView::CursorAction, modifiers: Rute::KeyboardModifiers) -> ModelIndexType,
    rect_for_index(index: &ModelIndexType) -> RectType,
    set_position_for_index(position: &PointType, index: &ModelIndexType),
    set_selection(rect: &RectType, command: ItemSelectionModel::SelectionFlags),
    visual_region_for_selection(selection: &ItemSelectionType) -> RegionType,
    selected_indexes() -> ModelIndexListType,
    is_index_hidden(index: &ModelIndexType) -> bool,
    selection_changed(selected: &ItemSelectionType, deselected: &ItemSelectionType),
    current_changed(current: &ModelIndexType, previous: &ModelIndexType),
    viewport_size_hint() -> SizeType,
}

