struct ListView : AbstractItemView {
    set_movement(ListView::Movement movement),
    movement() -> ListView::Movement,
    set_flow(ListView::Flow flow),
    flow() -> ListView::Flow,
    set_wrapping(bool enable),
    is_wrapping() -> bool,
    set_resize_mode(ListView::ResizeMode mode),
    resize_mode() -> ListView::ResizeMode,
    set_layout_mode(ListView::LayoutMode mode),
    layout_mode() -> ListView::LayoutMode,
    set_spacing(i32 space),
    spacing() -> i32,
    set_batch_size(i32 batch_size),
    batch_size() -> i32,
    set_grid_size(&SizeType size),
    grid_size() -> SizeType,
    set_view_mode(ListView::ViewMode mode),
    view_mode() -> ListView::ViewMode,
    clear_property_flags(),
    is_row_hidden(i32 row) -> bool,
    set_row_hidden(i32 row, bool hide),
    set_model_column(i32 column),
    model_column() -> i32,
    set_uniform_item_sizes(bool enable),
    uniform_item_sizes() -> bool,
    set_word_wrap(bool on),
    word_wrap() -> bool,
    set_selection_rect_visible(bool show),
    is_selection_rect_visible() -> bool,
    visual_rect(&ModelIndexType index) -> RectType,
    scroll_to(&ModelIndexType index, AbstractItemView::ScrollHint hint),
    index_at(&PointType p) -> ModelIndexType,
    do_items_layout(),
    reset(),
    set_root_index(&ModelIndexType index),
    indexes_moved(&ModelIndexListType indexes),
    event(&EventType e) -> bool,
    scroll_contents_by(i32 dx, i32 dy),
    resize_contents(i32 width, i32 height),
    contents_size() -> SizeType,
    data_changed(&ModelIndexType top_left, &ModelIndexType bottom_right, &Vector<int>Type roles),
    rows_inserted(&ModelIndexType parent, i32 start, i32 end),
    rows_about_to_be_removed(&ModelIndexType parent, i32 start, i32 end),
    mouse_move_event(&MouseEventType e),
    mouse_release_event(&MouseEventType e),
    wheel_event(&WheelEventType e),
    timer_event(&TimerEventType e),
    resize_event(&ResizeEventType e),
    drag_move_event(&DragMoveEventType e),
    drag_leave_event(&DragLeaveEventType e),
    drop_event(&DropEventType e),
    start_drag(Rute::DropActions supported_actions),
    view_options() -> StyleOptionViewItemType,
    paint_event(&PaintEventType e),
    horizontal_offset() -> i32,
    vertical_offset() -> i32,
    move_cursor(AbstractItemView::CursorAction cursor_action, Rute::KeyboardModifiers modifiers) -> ModelIndexType,
    rect_for_index(&ModelIndexType index) -> RectType,
    set_position_for_index(&PointType position, &ModelIndexType index),
    set_selection(&RectType rect, ItemSelectionModel::SelectionFlags command),
    visual_region_for_selection(&ItemSelectionType selection) -> RegionType,
    selected_indexes() -> ModelIndexListType,
    is_index_hidden(&ModelIndexType index) -> bool,
    selection_changed(&ItemSelectionType selected, &ItemSelectionType deselected),
    current_changed(&ModelIndexType current, &ModelIndexType previous),
    viewport_size_hint() -> SizeType,
    visual_index(&ModelIndexType index) -> i32,
}

