enum OpenModeFlag {
    NotOpen,
    ReadOnly,
    WriteOnly,
    ReadWrite,
    Append,
    Truncate,
    Text,
    Unbuffered,
}

struct IODevice : Object {
    open_mode() -> OpenMode,
    set_text_mode_enabled(enabled: bool),
    is_text_mode_enabled() -> bool,
    is_open() -> bool,
    is_readable() -> bool,
    is_writable() -> bool,
    [override] is_sequential() -> bool,
    read_channel_count() -> i32,
    write_channel_count() -> i32,
    current_read_channel() -> i32,
    set_current_read_channel(channel: i32),
    current_write_channel() -> i32,
    set_current_write_channel(channel: i32),
    [override] open(mode: OpenMode) -> bool,
    [override] close(),
    [override] pos() -> i64,
    [override] size() -> i64,
    [override] seek(pos: i64) -> bool,
    [override] at_end() -> bool,
    [override] reset() -> bool,
    [override] bytes_available() -> i64,
    [override] bytes_to_write() -> i64,
    read(data: &char, maxlen: i64) -> i64,
    read(maxlen: i64) -> ByteArrayType,
    read_all() -> ByteArrayType,
    read_line(data: &char, maxlen: i64) -> i64,
    read_line(maxlen: i64) -> ByteArrayType,
    [override] can_read_line() -> bool,
    start_transaction(),
    commit_transaction(),
    rollback_transaction(),
    is_transaction_started() -> bool,
    write(data: &char, len: i64) -> i64,
    write(data: &char) -> i64,
    write(data: &ByteArrayType) -> i64,
    peek(data: &char, maxlen: i64) -> i64,
    peek(maxlen: i64) -> ByteArrayType,
    skip(max_size: i64) -> i64,
    [override] wait_for_ready_read(msecs: i32) -> bool,
    [override] wait_for_bytes_written(msecs: i32) -> bool,
    unget_char(c: char),
    put_char(c: char) -> bool,
    get_char(c: &char) -> bool,
    [event] ready_read(),
    [event] channel_ready_read(channel: i32),
    [event] bytes_written(bytes: i64),
    [event] channel_bytes_written(channel: i32, bytes: i64),
    [event] about_to_close(),
    [event] read_channel_finished(),
    [override] read_data(data: &char, maxlen: i64) -> i64,
    [override] read_line_data(data: &char, maxlen: i64) -> i64,
    [override] write_data(data: &char, len: i64) -> i64,
    set_open_mode(open_mode: OpenMode),
}

